	public static function retrieveByPK($cd_municipio, $dt_versao_orc, $cd_orgao, $cd_unid_orc, $cd_rubrica_or, $nu_talao_receita, $dt_talao_receita, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_versao_orc, (string) $cd_orgao, (string) $cd_unid_orc, (string) $cd_rubrica_or, (string) $nu_talao_receita, (string) $dt_talao_receita));
 		if (null !== ($obj = TaloesReceitasPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(TaloesReceitasPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(TaloesReceitasPeer::DATABASE_NAME);
		$criteria->add(TaloesReceitasPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(TaloesReceitasPeer::DT_VERSAO_ORC, $dt_versao_orc);
		$criteria->add(TaloesReceitasPeer::CD_ORGAO, $cd_orgao);
		$criteria->add(TaloesReceitasPeer::CD_UNID_ORC, $cd_unid_orc);
		$criteria->add(TaloesReceitasPeer::CD_RUBRICA_OR, $cd_rubrica_or);
		$criteria->add(TaloesReceitasPeer::NU_TALAO_RECEITA, $nu_talao_receita);
		$criteria->add(TaloesReceitasPeer::DT_TALAO_RECEITA, $dt_talao_receita);
		$v = TaloesReceitasPeer::doSelect($criteria, $con);

