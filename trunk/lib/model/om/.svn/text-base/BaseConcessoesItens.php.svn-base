			if ($this->aItensRemuneratorios !== null) {
				if ($this->aItensRemuneratorios->isModified() || $this->aItensRemuneratorios->isNew()) {
					$affectedRows += $this->aItensRemuneratorios->save($con);
				}
				$this->setItensRemuneratorios($this->aItensRemuneratorios);
			}


			// If this object has been modified, then save it to the database.
			if ($this->isModified()) {
