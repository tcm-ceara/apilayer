			}

			if ($this->aAgentesPublicos !== null) {
				if ($this->aAgentesPublicos->isModified() || $this->aAgentesPublicos->isNew()) {
					$affectedRows += $this->aAgentesPublicos->save($con);
				}
				$this->setAgentesPublicos($this->aAgentesPublicos);
