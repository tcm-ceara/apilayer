				$criteria->add(AnulacTaloesExtrasPeer::DT_TALAO_RECEITA_TX, $this->dt_talao_receita_tx);


				$criteria->add(AnulacTaloesExtrasPeer::DT_VERSAO_ORC, $this->dt_versao_orc);


				$criteria->add(AnulacTaloesExtrasPeer::NU_TALAO_RECEITA_TX, $this->nu_talao_receita_tx);

				AnulacTaloesExtrasPeer::addSelectColumns($criteria);
				if (!isset($this->lastAnulacTaloesExtrasCriteria) || !$this->lastAnulacTaloesExtrasCriteria->equals($criteria)) {
					$this->collAnulacTaloesExtrass = AnulacTaloesExtrasPeer::doSelect($criteria, $con);
				}
			}
		}
		$this->lastAnulacTaloesExtrasCriteria = $criteria;
		return $this->collAnulacTaloesExtrass;
	}

	/**
	 * Returns the number of related AnulacTaloesExtras objects.
	 *
	 * @param      Criteria $criteria
	 * @param      boolean $distinct
	 * @param      PropelPDO $con
	 * @return     int Count of related AnulacTaloesExtras objects.
	 * @throws     PropelException
	 */
	public function countAnulacTaloesExtrass(Criteria $criteria = null, $distinct = false, PropelPDO $con = null)
	{
		if ($criteria === null) {
			$criteria = new Criteria(TaloesExtrasPeer::DATABASE_NAME);
		} else {
			$criteria = clone $criteria;
		}

		if ($distinct) {
			$criteria->setDistinct();
		}

		$count = null;

		if ($this->collAnulacTaloesExtrass === null) {
			if ($this->isNew()) {
				$count = 0;
			} else {

				$criteria->add(AnulacTaloesExtrasPeer::CD_MUNICIPIO, $this->cd_municipio);

				$criteria->add(AnulacTaloesExtrasPeer::CD_ORGAO, $this->cd_orgao);

				$criteria->add(AnulacTaloesExtrasPeer::CD_UNID_ORC, $this->cd_unid_orc);

				$criteria->add(AnulacTaloesExtrasPeer::DT_TALAO_RECEITA_TX, $this->dt_talao_receita_tx);

				$criteria->add(AnulacTaloesExtrasPeer::DT_VERSAO_ORC, $this->dt_versao_orc);

				$criteria->add(AnulacTaloesExtrasPeer::NU_TALAO_RECEITA_TX, $this->nu_talao_receita_tx);

				$count = AnulacTaloesExtrasPeer::doCount($criteria, false, $con);
			}
		} else {
			// criteria has no effect for a new object
			if (!$this->isNew()) {
				// the following code is to determine if a new query is
				// called for.  If the criteria is the same as the last
				// one, just return count of the collection.


				$criteria->add(AnulacTaloesExtrasPeer::CD_MUNICIPIO, $this->cd_municipio);


				$criteria->add(AnulacTaloesExtrasPeer::CD_ORGAO, $this->cd_orgao);


				$criteria->add(AnulacTaloesExtrasPeer::CD_UNID_ORC, $this->cd_unid_orc);


				$criteria->add(AnulacTaloesExtrasPeer::DT_TALAO_RECEITA_TX, $this->dt_talao_receita_tx);


				$criteria->add(AnulacTaloesExtrasPeer::DT_VERSAO_ORC, $this->dt_versao_orc);


				$criteria->add(AnulacTaloesExtrasPeer::NU_TALAO_RECEITA_TX, $this->nu_talao_receita_tx);

				if (!isset($this->lastAnulacTaloesExtrasCriteria) || !$this->lastAnulacTaloesExtrasCriteria->equals($criteria)) {
					$count = AnulacTaloesExtrasPeer::doCount($criteria, false, $con);
				} else {
					$count = count($this->collAnulacTaloesExtrass);
				}
			} else {
				$count = count($this->collAnulacTaloesExtrass);
			}
		}
		return $count;
	}

	/**
	 * Method called to associate a AnulacTaloesExtras object to this object
	 * through the AnulacTaloesExtras foreign key attribute.
	 *
	 * @param      AnulacTaloesExtras $l AnulacTaloesExtras
	 * @return     void
	 * @throws     PropelException
	 */
	public function addAnulacTaloesExtras(AnulacTaloesExtras $l)
	{
		if ($this->collAnulacTaloesExtrass === null) {
			$this->initAnulacTaloesExtrass();
		}
		if (!in_array($l, $this->collAnulacTaloesExtrass, true)) { // only add it if the **same** object is not already associated
			array_push($this->collAnulacTaloesExtrass, $l);
			$l->setTaloesExtras($this);
		}
	}

	/**
	 * Resets all collections of referencing foreign keys.
	 *
	 * This method is a user-space workaround for PHP's inability to garbage collect objects
	 * with circular references.  This is currently necessary when using Propel in certain
	 * daemon or large-volumne/high-memory operations.
	 *
	 * @param      boolean $deep Whether to also clear the references on all associated objects.
	 */
	public function clearAllReferences($deep = false)
	{
		if ($deep) {
			if ($this->collAnulacTaloesExtrass) {
				foreach ((array) $this->collAnulacTaloesExtrass as $o) {
					$o->clearAllReferences($deep);
				}
			}
		} // if ($deep)

		$this->collAnulacTaloesExtrass = null;
			$this->aUnidadesOrcamentarias = null;
			$this->aContasExtraOrc = null;
	}

	// symfony_behaviors behavior
	
	/**
	 * Calls methods defined via {@link sfMixer}.
	 */
	public function __call($method, $arguments)
	{
	  if (!$callable = sfMixer::getCallable('BaseTaloesExtras:'.$method))
	  {
	    throw new sfException(sprintf('Call to undefined method BaseTaloesExtras::%s', $method));
	  }
	
	  array_unshift($arguments, $this);
	
	  return call_user_func_array($callable, $arguments);
	}

