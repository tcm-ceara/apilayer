		if (!in_array($l, $this->collExtincoesItenss, true)) { // only add it if the **same** object is not already associated
			array_push($this->collExtincoesItenss, $l);
			$l->setItensRemuneratorios($this);
		}
	}


	/**
	 * If this collection has already been initialized with
	 * an identical criteria, it returns the collection.
	 * Otherwise if this ItensRemuneratorios is new, it will return
	 * an empty collection; or if this ItensRemuneratorios has previously
	 * been saved, it will retrieve related ExtincoesItenss from storage.
	 *
	 * This method is protected by default in order to keep the public
	 * api reasonable.  You can provide public methods for those you
	 * actually need in ItensRemuneratorios.
	 */
	public function getExtincoesItenssJoinTiposAmparosLegais($criteria = null, $con = null, $join_behavior = Criteria::LEFT_JOIN)
	{
		if ($criteria === null) {
			$criteria = new Criteria(ItensRemuneratoriosPeer::DATABASE_NAME);
		}
		elseif ($criteria instanceof Criteria)
		{
			$criteria = clone $criteria;
		}

		if ($this->collExtincoesItenss === null) {
			if ($this->isNew()) {
				$this->collExtincoesItenss = array();
			} else {

				$criteria->add(ExtincoesItensPeer::CD_MUNICIPIO, $this->cd_municipio);

				$this->collExtincoesItenss = ExtincoesItensPeer::doSelectJoinTiposAmparosLegais($criteria, $con, $join_behavior);
			}
		} else {
			// the following code is to determine if a new query is
			// called for.  If the criteria is the same as the last
			// one, just return the collection.

			$criteria->add(ExtincoesItensPeer::CD_MUNICIPIO, $this->cd_municipio);

			if (!isset($this->lastExtincoesItensCriteria) || !$this->lastExtincoesItensCriteria->equals($criteria)) {
				$this->collExtincoesItenss = ExtincoesItensPeer::doSelectJoinTiposAmparosLegais($criteria, $con, $join_behavior);
			}
		}
		$this->lastExtincoesItensCriteria = $criteria;

		return $this->collExtincoesItenss;
	}

	/**
	 * Resets all collections of referencing foreign keys.
	 *
	 * This method is a user-space workaround for PHP's inability to garbage collect objects
	 * with circular references.  This is currently necessary when using Propel in certain
	 * daemon or large-volumne/high-memory operations.
	 *
	 * @param      boolean $deep Whether to also clear the references on all associated objects.
	 */
	public function clearAllReferences($deep = false)
	{
		if ($deep) {
			if ($this->collConcessoesItenss) {
				foreach ((array) $this->collConcessoesItenss as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collExtincoesItenss) {
				foreach ((array) $this->collExtincoesItenss as $o) {
					$o->clearAllReferences($deep);
				}
			}
		} // if ($deep)

		$this->collConcessoesItenss = null;
		$this->collExtincoesItenss = null;
			$this->aTiposAmparosLegais = null;
	}

	// symfony_behaviors behavior
	
	/**
	 * Calls methods defined via {@link sfMixer}.
	 */
	public function __call($method, $arguments)
	{
	  if (!$callable = sfMixer::getCallable('BaseItensRemuneratorios:'.$method))
	  {
	    throw new sfException(sprintf('Call to undefined method BaseItensRemuneratorios::%s', $method));
	  }
	
	  array_unshift($arguments, $this);
	
	  return call_user_func_array($callable, $arguments);
	}

} // BaseItensRemuneratorios
