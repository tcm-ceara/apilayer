				$criteria->add(PensionistasSePeer::CD_ORGAO, $this->cd_orgao);

				$criteria->add(PensionistasSePeer::CD_UNID_ORC, $this->cd_unid_orc);

				$criteria->add(PensionistasSePeer::DT_VERSAO_ORC, $this->dt_versao_orc);

				$count = PensionistasSePeer::doCount($criteria, false, $con);
			}
		} else {
			// criteria has no effect for a new object
			if (!$this->isNew()) {
				// the following code is to determine if a new query is
				// called for.  If the criteria is the same as the last
				// one, just return count of the collection.


				$criteria->add(PensionistasSePeer::CD_MUNICIPIO, $this->cd_municipio);


				$criteria->add(PensionistasSePeer::CD_ORGAO, $this->cd_orgao);


				$criteria->add(PensionistasSePeer::CD_UNID_ORC, $this->cd_unid_orc);


				$criteria->add(PensionistasSePeer::DT_VERSAO_ORC, $this->dt_versao_orc);

				if (!isset($this->lastPensionistasSeCriteria) || !$this->lastPensionistasSeCriteria->equals($criteria)) {
					$count = PensionistasSePeer::doCount($criteria, false, $con);
				} else {
					$count = count($this->collPensionistasSes);
				}
			} else {
				$count = count($this->collPensionistasSes);
			}
		}
		return $count;
	}

	/**
	 * Method called to associate a PensionistasSe object to this object
	 * through the PensionistasSe foreign key attribute.
	 *
	 * @param      PensionistasSe $l PensionistasSe
	 * @return     void
	 * @throws     PropelException
	 */
	public function addPensionistasSe(PensionistasSe $l)
	{
		if ($this->collPensionistasSes === null) {
			$this->initPensionistasSes();
		}
		if (!in_array($l, $this->collPensionistasSes, true)) { // only add it if the **same** object is not already associated
			array_push($this->collPensionistasSes, $l);
			$l->setServidores($this);
		}
	}

	/**
	 * Resets all collections of referencing foreign keys.
	 *
	 * This method is a user-space workaround for PHP's inability to garbage collect objects
	 * with circular references.  This is currently necessary when using Propel in certain
	 * daemon or large-volumne/high-memory operations.
	 *
	 * @param      boolean $deep Whether to also clear the references on all associated objects.
	 */
	public function clearAllReferences($deep = false)
	{
		if ($deep) {
			if ($this->collAgentesPublicoss) {
				foreach ((array) $this->collAgentesPublicoss as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collDiariass) {
				foreach ((array) $this->collDiariass as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collPensionistasSes) {
				foreach ((array) $this->collPensionistasSes as $o) {
					$o->clearAllReferences($deep);
				}
			}
		} // if ($deep)

		$this->collAgentesPublicoss = null;
		$this->collDiariass = null;
		$this->collPensionistasSes = null;
			$this->aUnidadesOrcamentarias = null;
	}

	// symfony_behaviors behavior
	
	/**
	 * Calls methods defined via {@link sfMixer}.
	 */
	public function __call($method, $arguments)
	{
	  if (!$callable = sfMixer::getCallable('BaseServidores:'.$method))
	  {
	    throw new sfException(sprintf('Call to undefined method BaseServidores::%s', $method));
	  }
	
	  array_unshift($arguments, $this);
	
	  return call_user_func_array($callable, $arguments);
	}

} // BaseServidores
