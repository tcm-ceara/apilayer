	 */
	public static function doValidate(DadosGestao $obj, $cols = null)
	{
		$columns = array();

		if ($cols) {
			$dbMap = Propel::getDatabaseMap(DadosGestaoPeer::DATABASE_NAME);
			$tableMap = $dbMap->getTable(DadosGestaoPeer::TABLE_NAME);

			if (! is_array($cols)) {
				$cols = array($cols);
			}

			foreach ($cols as $colName) {
				if ($tableMap->containsColumn($colName)) {
					$get = 'get' . $tableMap->getColumn($colName)->getPhpName();
					$columns[$colName] = $obj->$get();
				}
			}
		} else {

		}

		return BasePeer::doValidate(DadosGestaoPeer::DATABASE_NAME, DadosGestaoPeer::TABLE_NAME, $columns);
	}

	/**
	 * Retrieve object using using composite pkey values.
	 * @param      string $cd_municipio
	 * @param      string $cd_cpf_gestor
	 * @param      string $dt_ini_gestao
	 * @param      PropelPDO $con
	 * @return     DadosGestao
	 */
	public static function retrieveByPK($cd_municipio, $cd_cpf_gestor, $dt_ini_gestao, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $cd_cpf_gestor, (string) $dt_ini_gestao));
 		if (null !== ($obj = DadosGestaoPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(DadosGestaoPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(DadosGestaoPeer::DATABASE_NAME);
		$criteria->add(DadosGestaoPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(DadosGestaoPeer::CD_CPF_GESTOR, $cd_cpf_gestor);
		$criteria->add(DadosGestaoPeer::DT_INI_GESTAO, $dt_ini_gestao);
		$v = DadosGestaoPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
	}
