	public static function retrieveByPK($cd_municipio, $dt_ini_obra_srv, $tp_obra_srv, $nu_obra_srv, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_ini_obra_srv, (string) $tp_obra_srv, (string) $nu_obra_srv));
 		if (null !== ($obj = ObrasServEngPeer::getInstanceFromPool($key))) {
