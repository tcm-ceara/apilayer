		if ($this->isColumnModified(ContratosPeer::DE_OBJETO_CONTRATO)) $criteria->add(ContratosPeer::DE_OBJETO_CONTRATO, $this->de_objeto_contrato);
		if ($this->isColumnModified(ContratosPeer::VL_VALOR_TOT_CONTRATO)) $criteria->add(ContratosPeer::VL_VALOR_TOT_CONTRATO, $this->vl_valor_tot_contrato);

		return $criteria;
