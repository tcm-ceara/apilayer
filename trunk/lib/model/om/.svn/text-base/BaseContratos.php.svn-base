		if ($this->isColumnModified(ContratosPeer::DT_CONTRATO)) $criteria->add(ContratosPeer::DT_CONTRATO, $this->dt_contrato);
		if ($this->isColumnModified(ContratosPeer::NU_CONTRATO)) $criteria->add(ContratosPeer::NU_CONTRATO, $this->nu_contrato);
		if ($this->isColumnModified(ContratosPeer::TP_CONTRATO)) $criteria->add(ContratosPeer::TP_CONTRATO, $this->tp_contrato);
