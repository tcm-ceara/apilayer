			if ($ret) {
				TaloesExtrasDelPeer::doDelete($this, $con);
				$this->postDelete($con);
				// symfony_behaviors behavior
				foreach (sfMixer::getCallables('BaseTaloesExtrasDel:delete:post') as $callable)
				{
