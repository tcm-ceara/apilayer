				// See http://propel.phpdb.org/trac/ticket/509
				// $obj1->hydrate($row, 0, true); // rehydrate
			} else {
				$cls = UnidadesGestorasPeer::getOMClass(false);

				$obj1 = new $cls();
				$obj1->hydrate($row);
				UnidadesGestorasPeer::addInstanceToPool($obj1, $key1);
			} // if obj1 already loaded

				// Add objects for joined DadosGestao rows

				$key2 = DadosGestaoPeer::getPrimaryKeyHashFromRow($row, $startcol2);
				if ($key2 !== null) {
					$obj2 = DadosGestaoPeer::getInstanceFromPool($key2);
					if (!$obj2) {
	
