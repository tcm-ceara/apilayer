		while ($row = $stmt->fetch(PDO::FETCH_NUM)) {
			$key1 = UnidadesGestorasPeer::getPrimaryKeyHashFromRow($row, 0);
			if (null !== ($obj1 = UnidadesGestorasPeer::getInstanceFromPool($key1))) {
