					$obj2 = new $cls();
					$obj2->hydrate($row, $startcol2);
					DadosGestaoPeer::addInstanceToPool($obj2, $key2);
				} // if $obj2 already loaded

				// Add the $obj1 (UnidadesGestoras) to the collection in $obj2 (DadosGestao)
				$obj2->addUnidadesGestoras($obj1);

			} // if joined row is not null

			$results[] = $obj1;
		}
		$stmt->closeCursor();
