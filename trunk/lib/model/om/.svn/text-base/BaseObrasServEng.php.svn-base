	 * daemon or large-volumne/high-memory operations.
	 *
	 * @param      boolean $deep Whether to also clear the references on all associated objects.
	 */
	public function clearAllReferences($deep = false)
	{
		if ($deep) {
			if ($this->collContratosObrass) {
				foreach ((array) $this->collContratosObrass as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collEmpenhosObrass) {
				foreach ((array) $this->collEmpenhosObrass as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collRespEngenObrass) {
				foreach ((array) $this->collRespEngenObrass as $o) {
					$o->clearAllReferences($deep);
				}
			}
		} // if ($deep)

		$this->collContratosObrass = null;
		$this->collEmpenhosObrass = null;
		$this->collRespEngenObrass = null;
	}

