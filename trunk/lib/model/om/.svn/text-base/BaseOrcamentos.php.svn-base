				$criteria->add(UnidadesGestorasPeer::DT_VERSAO_ORC, $this->dt_versao_orc);

				$this->collUnidadesGestorass = UnidadesGestorasPeer::doSelectJoinDadosGestao($criteria, $con, $join_behavior);
			}
		} else {
			// the following code is to determine if a new query is
			// called for.  If the criteria is the same as the last
			// one, just return the collection.

			$criteria->add(UnidadesGestorasPeer::CD_MUNICIPIO, $this->cd_municipio);

			$criteria->add(UnidadesGestorasPeer::DT_VERSAO_ORC, $this->dt_versao_orc);

			if (!isset($this->lastUnidadesGestorasCriteria) || !$this->lastUnidadesGestorasCriteria->equals($criteria)) {
				$this->collUnidadesGestorass = UnidadesGestorasPeer::doSelectJoinDadosGestao($criteria, $con, $join_behavior);
			}
		}
		$this->lastUnidadesGestorasCriteria = $criteria;

		return $this->collUnidadesGestorass;
	}

	/**
	 * Resets all collections of referencing foreign keys.
	 *
	 * This method is a user-space workaround for PHP's inability to garbage collect objects
	 * with circular references.  This is currently necessary when using Propel in certain
	 * daemon or large-volumne/high-memory operations.
	 *
	 * @param      boolean $deep Whether to also clear the references on all associated objects.
	 */
	public function clearAllReferences($deep = false)
	{
		if ($deep) {
			if ($this->collContasExtraOrcs) {
				foreach ((array) $this->collContasExtraOrcs as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collDadosOrcamentoss) {
				foreach ((array) $this->collDadosOrcamentoss as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collOrgaoss) {
				foreach ((array) $this->collOrgaoss as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collProgramass) {
				foreach ((array) $this->collProgramass as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collRealocacoesOrcamentariass) {
				foreach ((array) $this->collRealocacoesOrcamentariass as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collUnidGestorass) {
				foreach ((array) $this->collUnidGestorass as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collUnidadesGestorass) {
				foreach ((array) $this->collUnidadesGestorass as $o) {
					$o->clearAllReferences($deep);
				}
			}
		} // if ($deep)

		$this->collContasExtraOrcs = null;
		$this->collDadosOrcamentoss = null;
		$this->collOrgaoss = null;
		$this->collProgramass = null;
		$this->collRealocacoesOrcamentariass = null;
		$this->collUnidGestorass = null;
		$this->collUnidadesGestorass = null;
	}

	// symfony_behaviors behavior
	
	/**
	 * Calls methods defined via {@link sfMixer}.
	 */
	public function __call($method, $arguments)
	{
	  if (!$callable = sfMixer::getCallable('BaseOrcamentos:'.$method))
	  {
	    throw new sfException(sprintf('Call to undefined method BaseOrcamentos::%s', $method));
	  }
	
	  array_unshift($arguments, $this);
	
	  return call_user_func_array($callable, $arguments);
	}

} // BaseOrcamentos
