	public static function doInsert($values, PropelPDO $con = null)
	{
    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseContratosObrasPeer:doInsert:pre') as $sf_hook)
    {
      if (false !== $sf_hook_retval = call_user_func($sf_hook, 'BaseContratosObrasPeer', $values, $con))
      {
        return $sf_hook_retval;
      }
    }

		if ($con === null) {
			$con = Propel::getConnection(ContratosObrasPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		if ($values instanceof Criteria) {
			$criteria = clone $values; // rename for clarity
		} else {
			$criteria = $values->buildCriteria(); // build Criteria from ContratosObras object
		}


		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		try {
			// use transaction because $criteria could contain info
			// for more than one table (I guess, conceivably)
			$con->beginTransaction();
			$pk = BasePeer::doInsert($criteria, $con);
			$con->commit();
		} catch(PropelException $e) {
			$con->rollBack();
			throw $e;
		}

    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseContratosObrasPeer:doInsert:post') as $sf_hook)
    {
      call_user_func($sf_hook, 'BaseContratosObrasPeer', $values, $con, $pk);
    }

		return $pk;
	}

	/**
	 * Method perform an UPDATE on the database, given a ContratosObras or Criteria object.
	 *
	 * @param      mixed $values Criteria or ContratosObras object containing data that is used to create the UPDATE statement.
	 * @param      PropelPDO $con The connection to use (specify PropelPDO connection object to exert more control over transactions).
	 * @return     int The number of affected rows (if supported by underlying database driver).
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	public static function doUpdate($values, PropelPDO $con = null)
	{
    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseContratosObrasPeer:doUpdate:pre') as $sf_hook)
    {
      if (false !== $sf_hook_retval = call_user_func($sf_hook, 'BaseContratosObrasPeer', $values, $con))
      {
        return $sf_hook_retval;
      }
    }

		if ($con === null) {
			$con = Propel::getConnection(ContratosObrasPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		$selectCriteria = new Criteria(self::DATABASE_NAME);

		if ($values instanceof Criteria) {
			$criteria = clone $values; // rename for clarity

			$comparison = $criteria->getComparison(ContratosObrasPeer::CD_MUNICIPIO);
			$selectCriteria->add(ContratosObrasPeer::CD_MUNICIPIO, $criteria->remove(ContratosObrasPeer::CD_MUNICIPIO), $comparison);

			$comparison = $criteria->getComparison(ContratosObrasPeer::CD_CPF_GESTOR);
			$selectCriteria->add(ContratosObrasPeer::CD_CPF_GESTOR, $criteria->remove(ContratosObrasPeer::CD_CPF_GESTOR), $comparison);

			$comparison = $criteria->getComparison(ContratosObrasPeer::DT_CONTRATO);
			$selectCriteria->add(ContratosObrasPeer::DT_CONTRATO, $criteria->remove(ContratosObrasPeer::DT_CONTRATO), $comparison);

			$comparison = $criteria->getComparison(ContratosObrasPeer::NU_CONTRATO);
			$selectCriteria->add(ContratosObrasPeer::NU_CONTRATO, $criteria->remove(ContratosObrasPeer::NU_CONTRATO), $comparison);

			$comparison = $criteria->getComparison(ContratosObrasPeer::DT_INI_OBRA_SRV);
			$selectCriteria->add(ContratosObrasPeer::DT_INI_OBRA_SRV, $criteria->remove(ContratosObrasPeer::DT_INI_OBRA_SRV), $comparison);

			$comparison = $criteria->getComparison(ContratosObrasPeer::TP_OBRA_SRV);
			$selectCriteria->add(ContratosObrasPeer::TP_OBRA_SRV, $criteria->remove(ContratosObrasPeer::TP_OBRA_SRV), $comparison);

			$comparison = $criteria->getComparison(ContratosObrasPeer::NU_OBRA_SRV);
			$selectCriteria->add(ContratosObrasPeer::NU_OBRA_SRV, $criteria->remove(ContratosObrasPeer::NU_OBRA_SRV), $comparison);

		} else { // $values is ContratosObras object
			$criteria = $values->buildCriteria(); // gets full criteria
			$selectCriteria = $values->buildPkeyCriteria(); // gets criteria w/ primary key(s)
		}

		// set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$ret = BasePeer::doUpdate($selectCriteria, $criteria, $con);

    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseContratosObrasPeer:doUpdate:post') as $sf_hook)
    {
      call_user_func($sf_hook, 'BaseContratosObrasPeer', $values, $con, $ret);
    }

    return $ret;
	}

	/**
	 * Method to DELETE all rows from the CONTRATOS_OBRAS table.
	 *
	 * @return     int The number of affected rows (if supported by underlying database driver).
	 */
	public static function doDeleteAll($con = null)
	{
		if ($con === null) {
			$con = Propel::getConnection(ContratosObrasPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}
		$affectedRows = 0; // initialize var to track total num of affected rows
		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			$affectedRows += BasePeer::doDeleteAll(ContratosObrasPeer::TABLE_NAME, $con);
			// Because this db requires some delete cascade/set null emulation, we have to
			// clear the cached instance *after* the emulation has happened (since
			// instances get re-added by the select statement contained therein).
			ContratosObrasPeer::clearInstancePool();
			ContratosObrasPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

	/**
	 * Method perform a DELETE on the database, given a ContratosObras or Criteria object OR a primary key value.
	 *
	 * @param      mixed $values Criteria or ContratosObras object or primary key or array of primary keys
	 *              which is used to create the DELETE statement
	 * @param      PropelPDO $con the connection to use
	 * @return     int 	The number of affected rows (if supported by underlying database driver).  This includes CASCADE-related rows
	 *				if supported by native driver or if emulated using Propel.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	 public static function doDelete($values, PropelPDO $con = null)
	 {
		if ($con === null) {
			$con = Propel::getConnection(ContratosObrasPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		if ($values instanceof Criteria) {
			// invalidate the cache for all objects of this type, since we have no
			// way of knowing (without running a query) what objects should be invalidated
			// from the cache based on this Criteria.
			ContratosObrasPeer::clearInstancePool();
			// rename for clarity
			$criteria = clone $values;
		} elseif ($values instanceof ContratosObras) { // it's a model object
			// invalidate the cache for this single object
			ContratosObrasPeer::removeInstanceFromPool($values);
			// create criteria based on pk values
			$criteria = $values->buildPkeyCriteria();
		} else { // it's a primary key, or an array of pks
			$criteria = new Criteria(self::DATABASE_NAME);
			// primary key is composite; we therefore, expect
			// the primary key passed to be an array of pkey values
			if (count($values) == count($values, COUNT_RECURSIVE)) {
				// array is not multi-dimensional
				$values = array($values);
			}
			foreach ($values as $value) {
				$criterion = $criteria->getNewCriterion(ContratosObrasPeer::CD_MUNICIPIO, $value[0]);
				$criterion->addAnd($criteria->getNewCriterion(ContratosObrasPeer::CD_CPF_GESTOR, $value[1]));
				$criterion->addAnd($criteria->getNewCriterion(ContratosObrasPeer::DT_CONTRATO, $value[2]));
				$criterion->addAnd($criteria->getNewCriterion(ContratosObrasPeer::NU_CONTRATO, $value[3]));
				$criterion->addAnd($criteria->getNewCriterion(ContratosObrasPeer::DT_INI_OBRA_SRV, $value[4]));
				$criterion->addAnd($criteria->getNewCriterion(ContratosObrasPeer::TP_OBRA_SRV, $value[5]));
				$criterion->addAnd($criteria->getNewCriterion(ContratosObrasPeer::NU_OBRA_SRV, $value[6]));
				$criteria->addOr($criterion);
				// we can invalidate the cache for this single PK
				ContratosObrasPeer::removeInstanceFromPool($value);
			}
		}

		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$affectedRows = 0; // initialize var to track total num of affected rows

		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			
			$affectedRows += BasePeer::doDelete($criteria, $con);
			ContratosObrasPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

	/**
	 * Validates all modified columns of given ContratosObras object.
	 * If parameter $columns is either a single column name or an array of column names
	 * than only those columns are validated.
	 *
	 * NOTICE: This does not apply to primary or foreign keys for now.
	 *
	 * @param      ContratosObras $obj The object to validate.
	 * @param      mixed $cols Column name or array of column names.
	 *
	 * @return     mixed TRUE if all columns are valid or the error message of the first invalid column.
	 */
	public static function doValidate(ContratosObras $obj, $cols = null)
	{
		$columns = array();

		if ($cols) {
			$dbMap = Propel::getDatabaseMap(ContratosObrasPeer::DATABASE_NAME);
			$tableMap = $dbMap->getTable(ContratosObrasPeer::TABLE_NAME);

			if (! is_array($cols)) {
				$cols = array($cols);
			}

			foreach ($cols as $colName) {
				if ($tableMap->containsColumn($colName)) {
					$get = 'get' . $tableMap->getColumn($colName)->getPhpName();
					$columns[$colName] = $obj->$get();
				}
			}
		} else {

		}

		return BasePeer::doValidate(ContratosObrasPeer::DATABASE_NAME, ContratosObrasPeer::TABLE_NAME, $columns);
	}

	/**
	 * Retrieve object using using composite pkey values.
	 * @param      string $cd_municipio
	 * @param      string $cd_cpf_gestor
	 * @param      string $dt_contrato
	 * @param      string $nu_contrato
	 * @param      string $dt_ini_obra_srv
	 * @param      string $tp_obra_srv
	 * @param      int $nu_obra_srv
	 * @param      PropelPDO $con
	 * @return     ContratosObras
	 */
	public static function retrieveByPK($cd_municipio, $cd_cpf_gestor, $dt_contrato, $nu_contrato, $dt_ini_obra_srv, $tp_obra_srv, $nu_obra_srv, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $cd_cpf_gestor, (string) $dt_contrato, (string) $nu_contrato, (string) $dt_ini_obra_srv, (string) $tp_obra_srv, (string) $nu_obra_srv));
 		if (null !== ($obj = ContratosObrasPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(ContratosObrasPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(ContratosObrasPeer::DATABASE_NAME);
		$criteria->add(ContratosObrasPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(ContratosObrasPeer::CD_CPF_GESTOR, $cd_cpf_gestor);
		$criteria->add(ContratosObrasPeer::DT_CONTRATO, $dt_contrato);
		$criteria->add(ContratosObrasPeer::NU_CONTRATO, $nu_contrato);
		$criteria->add(ContratosObrasPeer::DT_INI_OBRA_SRV, $dt_ini_obra_srv);
		$criteria->add(ContratosObrasPeer::TP_OBRA_SRV, $tp_obra_srv);
		$criteria->add(ContratosObrasPeer::NU_OBRA_SRV, $nu_obra_srv);
		$v = ContratosObrasPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
	}
