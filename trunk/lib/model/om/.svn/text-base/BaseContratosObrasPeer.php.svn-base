				if ($key2 !== null) {
					$obj2 = ContratosPeer::getInstanceFromPool($key2);
					if (!$obj2) {
	
						$cls = ContratosPeer::getOMClass(false);

					$obj2 = new $cls();
					$obj2->hydrate($row, $startcol2);
					ContratosPeer::addInstanceToPool($obj2, $key2);
				} // if $obj2 already loaded

				// Add the $obj1 (ContratosObras) to the collection in $obj2 (Contratos)
				$obj2->addContratosObras($obj1);

			} // if joined row is not null

			$results[] = $obj1;
		}
		$stmt->closeCursor();
		return $results;
	}

