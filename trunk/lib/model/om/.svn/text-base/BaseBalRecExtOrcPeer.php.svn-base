			foreach ($values as $value) {
				$criterion = $criteria->getNewCriterion(BalRecExtOrcPeer::CD_MUNICIPIO, $value[0]);
				$criterion->addAnd($criteria->getNewCriterion(BalRecExtOrcPeer::DT_VERSAO_ORC, $value[1]));
