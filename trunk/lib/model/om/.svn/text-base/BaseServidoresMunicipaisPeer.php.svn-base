        array(ServidoresMunicipaisPeer::CD_MUNICIPIO, UnidadesOrcamentariasPeer::CD_MUNICIPIO),
        array(ServidoresMunicipaisPeer::CD_ORGAO, UnidadesOrcamentariasPeer::CD_ORGAO),
