	 */
	public static function retrieveByPK($cd_municipio, $dt_versao_orc, $cd_orgao, $cd_unid_orc, $cd_cpf_serv_muni, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_versao_orc, (string) $cd_orgao, (string) $cd_unid_orc, (string) $cd_cpf_serv_muni));
 		if (null !== ($obj = ServidoresMunicipaisPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(ServidoresMunicipaisPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(ServidoresMunicipaisPeer::DATABASE_NAME);
		$criteria->add(ServidoresMunicipaisPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(ServidoresMunicipaisPeer::DT_VERSAO_ORC, $dt_versao_orc);
		$criteria->add(ServidoresMunicipaisPeer::CD_ORGAO, $cd_orgao);
		$criteria->add(ServidoresMunicipaisPeer::CD_UNID_ORC, $cd_unid_orc);
		$criteria->add(ServidoresMunicipaisPeer::CD_CPF_SERV_MUNI, $cd_cpf_serv_muni);
		$v = ServidoresMunicipaisPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
