	 * @param      String    $join_behavior the type of joins to use, defaults to Criteria::LEFT_JOIN
	 * @return     int Number of matching rows.
	 */
	public static function doCountJoinAll(Criteria $criteria, $distinct = false, PropelPDO $con = null, $join_behavior = Criteria::LEFT_JOIN)
	{
		// we're going to modify criteria, so copy it first
		$criteria = clone $criteria;

		// We need to set the primary table name, since in the case that there are no WHERE columns
		// it will be impossible for the BasePeer::createSelectSql() method to determine which
		// tables go into the FROM clause.
		$criteria->setPrimaryTableName(PensionistasPeer::TABLE_NAME);

		if ($distinct && !in_array(Criteria::DISTINCT, $criteria->getSelectModifiers())) {
			$criteria->setDistinct();
		}

		if (!$criteria->hasSelectClause()) {
			PensionistasPeer::addSelectColumns($criteria);
		}
		
		$criteria->clearOrderByColumns(); // ORDER BY won't ever affect the count
		
		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		if ($con === null) {
			$con = Propel::getConnection(PensionistasPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}

		$criteria->addMultipleJoin(array(
        array(PensionistasPeer::CD_MUNICIPIO, ServidoresMunicipaisPeer::CD_MUNICIPIO),
        array(PensionistasPeer::CD_ORGAO, ServidoresMunicipaisPeer::CD_ORGAO),
        array(PensionistasPeer::CD_UNID_ORC, ServidoresMunicipaisPeer::CD_UNID_ORC),
        array(PensionistasPeer::DT_VERSAO_ORC, ServidoresMunicipaisPeer::DT_VERSAO_ORC),
      ), $join_behavior);

		// symfony_behaviors behavior
		foreach (sfMixer::getCallables(self::getMixerPreSelectHook(__FUNCTION__)) as $sf_hook)
		{
		  call_user_func($sf_hook, 'BasePensionistasPeer', $criteria, $con);
		}

		$stmt = BasePeer::doCount($criteria, $con);

		if ($row = $stmt->fetch(PDO::FETCH_NUM)) {
			$count = (int) $row[0];
		} else {
			$count = 0; // no rows returned; we infer that means 0 matches.
		}
		$stmt->closeCursor();
		return $count;
	}

	/**
	 * Selects a collection of Pensionistas objects pre-filled with all related objects.
	 *
	 * @param      Criteria  $criteria
	 * @param      PropelPDO $con
	 * @param      String    $join_behavior the type of joins to use, defaults to Criteria::LEFT_JOIN
	 * @return     array Array of Pensionistas objects.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	public static function doSelectJoinAll(Criteria $criteria, $con = null, $join_behavior = Criteria::LEFT_JOIN)
	{
		$criteria = clone $criteria;

		// Set the correct dbName if it has not been overridden
		if ($criteria->getDbName() == Propel::getDefaultDB()) {
			$criteria->setDbName(self::DATABASE_NAME);
		}

		PensionistasPeer::addSelectColumns($criteria);
		$startcol2 = (PensionistasPeer::NUM_COLUMNS - PensionistasPeer::NUM_LAZY_LOAD_COLUMNS);

		ServidoresMunicipaisPeer::addSelectColumns($criteria);
		$startcol3 = $startcol2 + (ServidoresMunicipaisPeer::NUM_COLUMNS - ServidoresMunicipaisPeer::NUM_LAZY_LOAD_COLUMNS);

		$criteria->addMultipleJoin(array(
        array(PensionistasPeer::CD_MUNICIPIO, ServidoresMunicipaisPeer::CD_MUNICIPIO),
        array(PensionistasPeer::CD_ORGAO, ServidoresMunicipaisPeer::CD_ORGAO),
        array(PensionistasPeer::CD_UNID_ORC, ServidoresMunicipaisPeer::CD_UNID_ORC),
        array(PensionistasPeer::DT_VERSAO_ORC, ServidoresMunicipaisPeer::DT_VERSAO_ORC),
      ), $join_behavior);

		// symfony_behaviors behavior
		foreach (sfMixer::getCallables(self::getMixerPreSelectHook(__FUNCTION__)) as $sf_hook)
		{
		  call_user_func($sf_hook, 'BasePensionistasPeer', $criteria, $con);
		}

		$stmt = BasePeer::doSelect($criteria, $con);
		$results = array();

		while ($row = $stmt->fetch(PDO::FETCH_NUM)) {
			$key1 = PensionistasPeer::getPrimaryKeyHashFromRow($row, 0);
			if (null !== ($obj1 = PensionistasPeer::getInstanceFromPool($key1))) {
				// We no longer rehydrate the object, since this can cause data loss.
				// See http://propel.phpdb.org/trac/ticket/509
				// $obj1->hydrate($row, 0, true); // rehydrate
			} else {
				$cls = PensionistasPeer::getOMClass(false);

				$obj1 = new $cls();
				$obj1->hydrate($row);
				PensionistasPeer::addInstanceToPool($obj1, $key1);
			} // if obj1 already loaded

			// Add objects for joined ServidoresMunicipais rows

			$key2 = ServidoresMunicipaisPeer::getPrimaryKeyHashFromRow($row, $startcol2);
			if ($key2 !== null) {
				$obj2 = ServidoresMunicipaisPeer::getInstanceFromPool($key2);
				if (!$obj2) {

					$cls = ServidoresMunicipaisPeer::getOMClass(false);

					$obj2 = new $cls();
					$obj2->hydrate($row, $startcol2);
					ServidoresMunicipaisPeer::addInstanceToPool($obj2, $key2);
				} // if obj2 loaded

				// Add the $obj1 (Pensionistas) to the collection in $obj2 (ServidoresMunicipais)
				$obj2->addPensionistas($obj1);
			} // if joined row not null

			$results[] = $obj1;
		}
		$stmt->closeCursor();
		return $results;
	}

