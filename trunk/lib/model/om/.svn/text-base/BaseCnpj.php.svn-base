	/**
	 * Build a Criteria object containing the values of all modified columns in this object.
	 *
	 * @return     Criteria The Criteria object containing all modified values.
	 */
	public function buildCriteria()
	{
		$criteria = new Criteria(CnpjPeer::DATABASE_NAME);

		if ($this->isColumnModified(CnpjPeer::CGF)) $criteria->add(CnpjPeer::CGF, $this->cgf);
		if ($this->isColumnModified(CnpjPeer::RAZAO_SOCIAL)) $criteria->add(CnpjPeer::RAZAO_SOCIAL, $this->razao_social);
		if ($this->isColumnModified(CnpjPeer::ENDERECO)) $criteria->add(CnpjPeer::ENDERECO, $this->endereco);
		if ($this->isColumnModified(CnpjPeer::CEP)) $criteria->add(CnpjPeer::CEP, $this->cep);
		if ($this->isColumnModified(CnpjPeer::TEL_EMP)) $criteria->add(CnpjPeer::TEL_EMP, $this->tel_emp);
		if ($this->isColumnModified(CnpjPeer::CGC)) $criteria->add(CnpjPeer::CGC, $this->cgc);
		if ($this->isColumnModified(CnpjPeer::DAT_INI_AT)) $criteria->add(CnpjPeer::DAT_INI_AT, $this->dat_ini_at);
		if ($this->isColumnModified(CnpjPeer::NUM_JUNTA)) $criteria->add(CnpjPeer::NUM_JUNTA, $this->num_junta);
		if ($this->isColumnModified(CnpjPeer::DATA_BAIXA)) $criteria->add(CnpjPeer::DATA_BAIXA, $this->data_baixa);
		if ($this->isColumnModified(CnpjPeer::CPF_CONTADOR)) $criteria->add(CnpjPeer::CPF_CONTADOR, $this->cpf_contador);
		if ($this->isColumnModified(CnpjPeer::ID)) $criteria->add(CnpjPeer::ID, $this->id);

		return $criteria;
	}

