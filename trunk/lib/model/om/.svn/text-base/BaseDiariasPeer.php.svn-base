	/**
	 * Method perform an INSERT on the database, given a Diarias or Criteria object.
	 *
	 * @param      mixed $values Criteria or Diarias object containing data that is used to create the INSERT statement.
	 * @param      PropelPDO $con the PropelPDO connection to use
	 * @return     mixed The new primary key.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	public static function doInsert($values, PropelPDO $con = null)
	{
    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseDiariasPeer:doInsert:pre') as $sf_hook)
    {
      if (false !== $sf_hook_retval = call_user_func($sf_hook, 'BaseDiariasPeer', $values, $con))
      {
        return $sf_hook_retval;
      }
    }

		if ($con === null) {
			$con = Propel::getConnection(DiariasPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		if ($values instanceof Criteria) {
			$criteria = clone $values; // rename for clarity
		} else {
			$criteria = $values->buildCriteria(); // build Criteria from Diarias object
		}


		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		try {
			// use transaction because $criteria could contain info
			// for more than one table (I guess, conceivably)
			$con->beginTransaction();
			$pk = BasePeer::doInsert($criteria, $con);
			$con->commit();
		} catch(PropelException $e) {
			$con->rollBack();
			throw $e;
		}

    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseDiariasPeer:doInsert:post') as $sf_hook)
    {
      call_user_func($sf_hook, 'BaseDiariasPeer', $values, $con, $pk);
    }

		return $pk;
	}

	/**
	 * Method perform an UPDATE on the database, given a Diarias or Criteria object.
	 *
	 * @param      mixed $values Criteria or Diarias object containing data that is used to create the UPDATE statement.
	 * @param      PropelPDO $con The connection to use (specify PropelPDO connection object to exert more control over transactions).
	 * @return     int The number of affected rows (if supported by underlying database driver).
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	public static function doUpdate($values, PropelPDO $con = null)
	{
    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseDiariasPeer:doUpdate:pre') as $sf_hook)
    {
      if (false !== $sf_hook_retval = call_user_func($sf_hook, 'BaseDiariasPeer', $values, $con))
      {
        return $sf_hook_retval;
      }
    }

		if ($con === null) {
			$con = Propel::getConnection(DiariasPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		$selectCriteria = new Criteria(self::DATABASE_NAME);

		if ($values instanceof Criteria) {
			$criteria = clone $values; // rename for clarity

			$comparison = $criteria->getComparison(DiariasPeer::CD_MUNICIPIO);
			$selectCriteria->add(DiariasPeer::CD_MUNICIPIO, $criteria->remove(DiariasPeer::CD_MUNICIPIO), $comparison);

			$comparison = $criteria->getComparison(DiariasPeer::DT_VERSAO_ORC);
			$selectCriteria->add(DiariasPeer::DT_VERSAO_ORC, $criteria->remove(DiariasPeer::DT_VERSAO_ORC), $comparison);

			$comparison = $criteria->getComparison(DiariasPeer::CD_ORGAO);
			$selectCriteria->add(DiariasPeer::CD_ORGAO, $criteria->remove(DiariasPeer::CD_ORGAO), $comparison);

			$comparison = $criteria->getComparison(DiariasPeer::CD_UNID_ORC);
			$selectCriteria->add(DiariasPeer::CD_UNID_ORC, $criteria->remove(DiariasPeer::CD_UNID_ORC), $comparison);

			$comparison = $criteria->getComparison(DiariasPeer::CD_CPF_SERV_MUNI);
			$selectCriteria->add(DiariasPeer::CD_CPF_SERV_MUNI, $criteria->remove(DiariasPeer::CD_CPF_SERV_MUNI), $comparison);

			$comparison = $criteria->getComparison(DiariasPeer::NU_PORTARIA_DA);
			$selectCriteria->add(DiariasPeer::NU_PORTARIA_DA, $criteria->remove(DiariasPeer::NU_PORTARIA_DA), $comparison);

		} else { // $values is Diarias object
			$criteria = $values->buildCriteria(); // gets full criteria
			$selectCriteria = $values->buildPkeyCriteria(); // gets criteria w/ primary key(s)
		}

		// set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$ret = BasePeer::doUpdate($selectCriteria, $criteria, $con);

    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseDiariasPeer:doUpdate:post') as $sf_hook)
    {
      call_user_func($sf_hook, 'BaseDiariasPeer', $values, $con, $ret);
    }

    return $ret;
	}

	/**
	 * Method to DELETE all rows from the DIARIAS table.
	 *
	 * @return     int The number of affected rows (if supported by underlying database driver).
	 */
	public static function doDeleteAll($con = null)
	{
		if ($con === null) {
			$con = Propel::getConnection(DiariasPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}
		$affectedRows = 0; // initialize var to track total num of affected rows
		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			$affectedRows += BasePeer::doDeleteAll(DiariasPeer::TABLE_NAME, $con);
			// Because this db requires some delete cascade/set null emulation, we have to
			// clear the cached instance *after* the emulation has happened (since
			// instances get re-added by the select statement contained therein).
			DiariasPeer::clearInstancePool();
			DiariasPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

	/**
	 * Method perform a DELETE on the database, given a Diarias or Criteria object OR a primary key value.
	 *
	 * @param      mixed $values Criteria or Diarias object or primary key or array of primary keys
	 *              which is used to create the DELETE statement
	 * @param      PropelPDO $con the connection to use
	 * @return     int 	The number of affected rows (if supported by underlying database driver).  This includes CASCADE-related rows
	 *				if supported by native driver or if emulated using Propel.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	 public static function doDelete($values, PropelPDO $con = null)
	 {
		if ($con === null) {
			$con = Propel::getConnection(DiariasPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		if ($values instanceof Criteria) {
			// invalidate the cache for all objects of this type, since we have no
			// way of knowing (without running a query) what objects should be invalidated
			// from the cache based on this Criteria.
			DiariasPeer::clearInstancePool();
			// rename for clarity
			$criteria = clone $values;
		} elseif ($values instanceof Diarias) { // it's a model object
			// invalidate the cache for this single object
			DiariasPeer::removeInstanceFromPool($values);
			// create criteria based on pk values
			$criteria = $values->buildPkeyCriteria();
		} else { // it's a primary key, or an array of pks
			$criteria = new Criteria(self::DATABASE_NAME);
			// primary key is composite; we therefore, expect
			// the primary key passed to be an array of pkey values
			if (count($values) == count($values, COUNT_RECURSIVE)) {
				// array is not multi-dimensional
				$values = array($values);
			}
			foreach ($values as $value) {
				$criterion = $criteria->getNewCriterion(DiariasPeer::CD_MUNICIPIO, $value[0]);
				$criterion->addAnd($criteria->getNewCriterion(DiariasPeer::DT_VERSAO_ORC, $value[1]));
				$criterion->addAnd($criteria->getNewCriterion(DiariasPeer::CD_ORGAO, $value[2]));
				$criterion->addAnd($criteria->getNewCriterion(DiariasPeer::CD_UNID_ORC, $value[3]));
				$criterion->addAnd($criteria->getNewCriterion(DiariasPeer::CD_CPF_SERV_MUNI, $value[4]));
				$criterion->addAnd($criteria->getNewCriterion(DiariasPeer::NU_PORTARIA_DA, $value[5]));
				$criteria->addOr($criterion);
				// we can invalidate the cache for this single PK
				DiariasPeer::removeInstanceFromPool($value);
			}
		}

		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$affectedRows = 0; // initialize var to track total num of affected rows

		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			
			$affectedRows += BasePeer::doDelete($criteria, $con);
			DiariasPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

	/**
	 * Validates all modified columns of given Diarias object.
	 * If parameter $columns is either a single column name or an array of column names
	 * than only those columns are validated.
	 *
	 * NOTICE: This does not apply to primary or foreign keys for now.
	 *
	 * @param      Diarias $obj The object to validate.
	 * @param      mixed $cols Column name or array of column names.
	 *
	 * @return     mixed TRUE if all columns are valid or the error message of the first invalid column.
	 */
	public static function doValidate(Diarias $obj, $cols = null)
	{
		$columns = array();

		if ($cols) {
			$dbMap = Propel::getDatabaseMap(DiariasPeer::DATABASE_NAME);
			$tableMap = $dbMap->getTable(DiariasPeer::TABLE_NAME);

			if (! is_array($cols)) {
				$cols = array($cols);
			}

			foreach ($cols as $colName) {
				if ($tableMap->containsColumn($colName)) {
					$get = 'get' . $tableMap->getColumn($colName)->getPhpName();
					$columns[$colName] = $obj->$get();
				}
			}
		} else {

		}

		return BasePeer::doValidate(DiariasPeer::DATABASE_NAME, DiariasPeer::TABLE_NAME, $columns);
	}

	/**
	 * Retrieve object using using composite pkey values.
	 * @param      string $cd_municipio
	 * @param      int $dt_versao_orc
	 * @param      string $cd_orgao
	 * @param      string $cd_unid_orc
	 * @param      string $cd_cpf_serv_muni
	 * @param      string $nu_portaria_da
	 * @param      PropelPDO $con
	 * @return     Diarias
	 */
	public static function retrieveByPK($cd_municipio, $dt_versao_orc, $cd_orgao, $cd_unid_orc, $cd_cpf_serv_muni, $nu_portaria_da, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_versao_orc, (string) $cd_orgao, (string) $cd_unid_orc, (string) $cd_cpf_serv_muni, (string) $nu_portaria_da));
 		if (null !== ($obj = DiariasPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(DiariasPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(DiariasPeer::DATABASE_NAME);
		$criteria->add(DiariasPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(DiariasPeer::DT_VERSAO_ORC, $dt_versao_orc);
		$criteria->add(DiariasPeer::CD_ORGAO, $cd_orgao);
		$criteria->add(DiariasPeer::CD_UNID_ORC, $cd_unid_orc);
		$criteria->add(DiariasPeer::CD_CPF_SERV_MUNI, $cd_cpf_serv_muni);
		$criteria->add(DiariasPeer::NU_PORTARIA_DA, $nu_portaria_da);
		$v = DiariasPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
	}
