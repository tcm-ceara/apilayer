	 * Selects a collection of ContasExtraOrc objects pre-filled with all related objects.
	 *
	 * @param      Criteria  $criteria
	 * @param      PropelPDO $con
	 * @param      String    $join_behavior the type of joins to use, defaults to Criteria::LEFT_JOIN
	 * @return     array Array of ContasExtraOrc objects.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	public static function doSelectJoinAll(Criteria $criteria, $con = null, $join_behavior = Criteria::LEFT_JOIN)
	{
		$criteria = clone $criteria;

		// Set the correct dbName if it has not been overridden
		if ($criteria->getDbName() == Propel::getDefaultDB()) {
			$criteria->setDbName(self::DATABASE_NAME);
		}

		ContasExtraOrcPeer::addSelectColumns($criteria);
		$startcol2 = (ContasExtraOrcPeer::NUM_COLUMNS - ContasExtraOrcPeer::NUM_LAZY_LOAD_COLUMNS);

		OrcamentosPeer::addSelectColumns($criteria);
		$startcol3 = $startcol2 + (OrcamentosPeer::NUM_COLUMNS - OrcamentosPeer::NUM_LAZY_LOAD_COLUMNS);

		$criteria->addJoin(ContasExtraOrcPeer::DT_VERSAO_ORC, OrcamentosPeer::DT_VERSAO_ORC, $join_behavior);

		// symfony_behaviors behavior
		foreach (sfMixer::getCallables(self::getMixerPreSelectHook(__FUNCTION__)) as $sf_hook)
		{
		  call_user_func($sf_hook, 'BaseContasExtraOrcPeer', $criteria, $con);
		}

		$stmt = BasePeer::doSelect($criteria, $con);
		$results = array();

		while ($row = $stmt->fetch(PDO::FETCH_NUM)) {
			$key1 = ContasExtraOrcPeer::getPrimaryKeyHashFromRow($row, 0);
			if (null !== ($obj1 = ContasExtraOrcPeer::getInstanceFromPool($key1))) {
				// We no longer rehydrate the object, since this can cause data loss.
				// See http://propel.phpdb.org/trac/ticket/509
				// $obj1->hydrate($row, 0, true); // rehydrate
			} else {
				$cls = ContasExtraOrcPeer::getOMClass(false);

				$obj1 = new $cls();
				$obj1->hydrate($row);
				ContasExtraOrcPeer::addInstanceToPool($obj1, $key1);
			} // if obj1 already loaded

			// Add objects for joined Orcamentos rows

			$key2 = OrcamentosPeer::getPrimaryKeyHashFromRow($row, $startcol2);
			if ($key2 !== null) {
				$obj2 = OrcamentosPeer::getInstanceFromPool($key2);
				if (!$obj2) {

					$cls = OrcamentosPeer::getOMClass(false);

					$obj2 = new $cls();
					$obj2->hydrate($row, $startcol2);
					OrcamentosPeer::addInstanceToPool($obj2, $key2);
				} // if obj2 loaded

				// Add the $obj1 (ContasExtraOrc) to the collection in $obj2 (Orcamentos)
				$obj2->addContasExtraOrc($obj1);
			} // if joined row not null

			$results[] = $obj1;
		}
		$stmt->closeCursor();
		return $results;
	}

