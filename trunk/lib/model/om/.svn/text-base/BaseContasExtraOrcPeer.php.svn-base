			$selectCriteria->add(ContasExtraOrcPeer::CD_CONTA_CTX, $criteria->remove(ContasExtraOrcPeer::CD_CONTA_CTX), $comparison);

		} else { // $values is ContasExtraOrc object
			$criteria = $values->buildCriteria(); // gets full criteria
			$selectCriteria = $values->buildPkeyCriteria(); // gets criteria w/ primary key(s)
		}

