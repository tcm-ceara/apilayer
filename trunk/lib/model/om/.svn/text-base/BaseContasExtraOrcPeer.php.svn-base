			$tableMap = $dbMap->getTable(ContasExtraOrcPeer::TABLE_NAME);

			if (! is_array($cols)) {
				$cols = array($cols);
			}

			foreach ($cols as $colName) {
				if ($tableMap->containsColumn($colName)) {
					$get = 'get' . $tableMap->getColumn($colName)->getPhpName();
					$columns[$colName] = $obj->$get();
				}
			}
		} else {

		}

		return BasePeer::doValidate(ContasExtraOrcPeer::DATABASE_NAME, ContasExtraOrcPeer::TABLE_NAME, $columns);
	}

	/**
	 * Retrieve object using using composite pkey values.
	 * @param      string $cd_municipio
	 * @param      int $dt_versao_orc
	 * @param      int $cd_conta_ctx
	 * @param      PropelPDO $con
	 * @return     ContasExtraOrc
	 */
	public static function retrieveByPK($cd_municipio, $dt_versao_orc, $cd_conta_ctx, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_versao_orc, (string) $cd_conta_ctx));
 		if (null !== ($obj = ContasExtraOrcPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(ContasExtraOrcPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(ContasExtraOrcPeer::DATABASE_NAME);
		$criteria->add(ContasExtraOrcPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(ContasExtraOrcPeer::DT_VERSAO_ORC, $dt_versao_orc);
		$criteria->add(ContasExtraOrcPeer::CD_CONTA_CTX, $cd_conta_ctx);
		$v = ContasExtraOrcPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
	}
