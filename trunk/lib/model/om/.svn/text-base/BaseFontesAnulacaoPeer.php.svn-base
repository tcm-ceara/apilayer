				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::DT_VERSAO_ORC, $value[1]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::DT_ABERT_CRED_CA, $value[2]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::NU_SEQ_NO_DIA_CA, $value[3]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::CD_ORGAO_ANUL_FA, $value[4]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::CD_UNID_ORC_ANUL_FA, $value[5]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::CD_FUNCAO_ANUL_FA, $value[6]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::CD_SUBFUNCAO_ANUL_FA, $value[7]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::CD_PROGRAMA_ANUL_FA, $value[8]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::CD_PROJ_ATIV_ANUL_FA, $value[9]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::NU_PROJ_ATIV_ANUL_FA, $value[10]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::NU_SUB_PROJ_ATIV_ANUL_FA, $value[11]));
				$criterion->addAnd($criteria->getNewCriterion(FontesAnulacaoPeer::CD_ELEMENTO_OD_ANUL_FA, $value[12]));
				$criteria->addOr($criterion);
				// we can invalidate the cache for this single PK
				FontesAnulacaoPeer::removeInstanceFromPool($value);
			}
		}

		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$affectedRows = 0; // initialize var to track total num of affected rows

		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			
			$affectedRows += BasePeer::doDelete($criteria, $con);
			FontesAnulacaoPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

