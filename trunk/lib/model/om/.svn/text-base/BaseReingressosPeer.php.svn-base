			$selectCriteria->add(ReingressosPeer::NU_EXPEDIENTE_NO, $criteria->remove(ReingressosPeer::NU_EXPEDIENTE_NO), $comparison);

			$comparison = $criteria->getComparison(ReingressosPeer::NU_EXPEDIENTE_DE);
			$selectCriteria->add(ReingressosPeer::NU_EXPEDIENTE_DE, $criteria->remove(ReingressosPeer::NU_EXPEDIENTE_DE), $comparison);

			$comparison = $criteria->getComparison(ReingressosPeer::NU_EXPEDIENTE_RE);
			$selectCriteria->add(ReingressosPeer::NU_EXPEDIENTE_RE, $criteria->remove(ReingressosPeer::NU_EXPEDIENTE_RE), $comparison);

		} else { // $values is Reingressos object
			$criteria = $values->buildCriteria(); // gets full criteria
			$selectCriteria = $values->buildPkeyCriteria(); // gets criteria w/ primary key(s)
		}

		// set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$ret = BasePeer::doUpdate($selectCriteria, $criteria, $con);

    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseReingressosPeer:doUpdate:post') as $sf_hook)
    {
      call_user_func($sf_hook, 'BaseReingressosPeer', $values, $con, $ret);
    }

    return $ret;
	}

	/**
	 * Method to DELETE all rows from the REINGRESSOS table.
	 *
	 * @return     int The number of affected rows (if supported by underlying database driver).
	 */
	public static function doDeleteAll($con = null)
	{
		if ($con === null) {
			$con = Propel::getConnection(ReingressosPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}
		$affectedRows = 0; // initialize var to track total num of affected rows
		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			$affectedRows += BasePeer::doDeleteAll(ReingressosPeer::TABLE_NAME, $con);
			// Because this db requires some delete cascade/set null emulation, we have to
			// clear the cached instance *after* the emulation has happened (since
			// instances get re-added by the select statement contained therein).
			ReingressosPeer::clearInstancePool();
			ReingressosPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

	/**
	 * Method perform a DELETE on the database, given a Reingressos or Criteria object OR a primary key value.
	 *
	 * @param      mixed $values Criteria or Reingressos object or primary key or array of primary keys
	 *              which is used to create the DELETE statement
	 * @param      PropelPDO $con the connection to use
	 * @return     int 	The number of affected rows (if supported by underlying database driver).  This includes CASCADE-related rows
	 *				if supported by native driver or if emulated using Propel.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	 public static function doDelete($values, PropelPDO $con = null)
	 {
		if ($con === null) {
			$con = Propel::getConnection(ReingressosPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		if ($values instanceof Criteria) {
			// invalidate the cache for all objects of this type, since we have no
			// way of knowing (without running a query) what objects should be invalidated
			// from the cache based on this Criteria.
			ReingressosPeer::clearInstancePool();
			// rename for clarity
			$criteria = clone $values;
		} elseif ($values instanceof Reingressos) { // it's a model object
			// invalidate the cache for this single object
			ReingressosPeer::removeInstanceFromPool($values);
			// create criteria based on pk values
			$criteria = $values->buildPkeyCriteria();
		} else { // it's a primary key, or an array of pks
			$criteria = new Criteria(self::DATABASE_NAME);
			// primary key is composite; we therefore, expect
			// the primary key passed to be an array of pkey values
			if (count($values) == count($values, COUNT_RECURSIVE)) {
				// array is not multi-dimensional
				$values = array($values);
			}
			foreach ($values as $value) {
				$criterion = $criteria->getNewCriterion(ReingressosPeer::CD_MUNICIPIO, $value[0]);
				$criterion->addAnd($criteria->getNewCriterion(ReingressosPeer::DT_VERSAO_ORC, $value[1]));
				$criterion->addAnd($criteria->getNewCriterion(ReingressosPeer::CD_ORGAO, $value[2]));
				$criterion->addAnd($criteria->getNewCriterion(ReingressosPeer::CD_UNID_ORC, $value[3]));
				$criterion->addAnd($criteria->getNewCriterion(ReingressosPeer::CD_CPF_SERV_MUNI, $value[4]));
				$criterion->addAnd($criteria->getNewCriterion(ReingressosPeer::CD_INGRESSO, $value[5]));
				$criterion->addAnd($criteria->getNewCriterion(ReingressosPeer::CD_VINCULO, $value[6]));
				$criterion->addAnd($criteria->getNewCriterion(ReingressosPeer::NU_EXPEDIENTE_NO, $value[7]));
				$criterion->addAnd($criteria->getNewCriterion(ReingressosPeer::NU_EXPEDIENTE_DE, $value[8]));
				$criterion->addAnd($criteria->getNewCriterion(ReingressosPeer::NU_EXPEDIENTE_RE, $value[9]));
				$criteria->addOr($criterion);
				// we can invalidate the cache for this single PK
				ReingressosPeer::removeInstanceFromPool($value);
			}
		}

		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$affectedRows = 0; // initialize var to track total num of affected rows

		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			
			$affectedRows += BasePeer::doDelete($criteria, $con);
			ReingressosPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

	/**
	 * Validates all modified columns of given Reingressos object.
	 * If parameter $columns is either a single column name or an array of column names
	 * than only those columns are validated.
	 *
	 * NOTICE: This does not apply to primary or foreign keys for now.
	 *
	 * @param      Reingressos $obj The object to validate.
	 * @param      mixed $cols Column name or array of column names.
	 *
	 * @return     mixed TRUE if all columns are valid or the error message of the first invalid column.
	 */
	public static function doValidate(Reingressos $obj, $cols = null)
	{
		$columns = array();

		if ($cols) {
			$dbMap = Propel::getDatabaseMap(ReingressosPeer::DATABASE_NAME);
			$tableMap = $dbMap->getTable(ReingressosPeer::TABLE_NAME);

			if (! is_array($cols)) {
				$cols = array($cols);
			}

			foreach ($cols as $colName) {
				if ($tableMap->containsColumn($colName)) {
					$get = 'get' . $tableMap->getColumn($colName)->getPhpName();
					$columns[$colName] = $obj->$get();
				}
			}
		} else {

		}

		return BasePeer::doValidate(ReingressosPeer::DATABASE_NAME, ReingressosPeer::TABLE_NAME, $columns);
	}

	/**
	 * Retrieve object using using composite pkey values.
	 * @param      string $cd_municipio
	 * @param      int $dt_versao_orc
	 * @param      string $cd_orgao
	 * @param      string $cd_unid_orc
	 * @param      string $cd_cpf_serv_muni
	 * @param      string $cd_ingresso
	 * @param      string $cd_vinculo
	 * @param      string $nu_expediente_no
	 * @param      string $nu_expediente_de
	 * @param      string $nu_expediente_re
	 * @param      PropelPDO $con
	 * @return     Reingressos
	 */
	public static function retrieveByPK($cd_municipio, $dt_versao_orc, $cd_orgao, $cd_unid_orc, $cd_cpf_serv_muni, $cd_ingresso, $cd_vinculo, $nu_expediente_no, $nu_expediente_de, $nu_expediente_re, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_versao_orc, (string) $cd_orgao, (string) $cd_unid_orc, (string) $cd_cpf_serv_muni, (string) $cd_ingresso, (string) $cd_vinculo, (string) $nu_expediente_no, (string) $nu_expediente_de, (string) $nu_expediente_re));
 		if (null !== ($obj = ReingressosPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(ReingressosPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(ReingressosPeer::DATABASE_NAME);
		$criteria->add(ReingressosPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(ReingressosPeer::DT_VERSAO_ORC, $dt_versao_orc);
		$criteria->add(ReingressosPeer::CD_ORGAO, $cd_orgao);
		$criteria->add(ReingressosPeer::CD_UNID_ORC, $cd_unid_orc);
		$criteria->add(ReingressosPeer::CD_CPF_SERV_MUNI, $cd_cpf_serv_muni);
		$criteria->add(ReingressosPeer::CD_INGRESSO, $cd_ingresso);
		$criteria->add(ReingressosPeer::CD_VINCULO, $cd_vinculo);
		$criteria->add(ReingressosPeer::NU_EXPEDIENTE_NO, $nu_expediente_no);
		$criteria->add(ReingressosPeer::NU_EXPEDIENTE_DE, $nu_expediente_de);
		$criteria->add(ReingressosPeer::NU_EXPEDIENTE_RE, $nu_expediente_re);
		$v = ReingressosPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
	}
