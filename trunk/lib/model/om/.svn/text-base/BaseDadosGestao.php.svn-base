					$affectedRows += $this->aGestores->save($con);
				}
				$this->setGestores($this->aGestores);
			}

			if ($this->aTiposCargos !== null) {
				if ($this->aTiposCargos->isModified() || $this->aTiposCargos->isNew()) {
					$affectedRows += $this->aTiposCargos->save($con);
				}
				$this->setTiposCargos($this->aTiposCargos);
			}


			// If this object has been modified, then save it to the database.
			if ($this->isModified()) {
				if ($this->isNew()) {
					$pk = DadosGestaoPeer::doInsert($this, $con);
					$affectedRows += 1; // we are assuming that there is only 1 row per doInsert() which
										 // should always be true here (even though technically
										 // BasePeer::doInsert() can insert multiple rows).

					$this->setNew(false);
				} else {
					$affectedRows += DadosGestaoPeer::doUpdate($this, $con);
				}

				$this->resetModified(); // [HL] After being saved an object is no longer 'modified'
			}

			if ($this->collGestSubstitutoss !== null) {
				foreach ($this->collGestSubstitutoss as $referrerFK) {
					if (!$referrerFK->isDeleted()) {
						$affectedRows += $referrerFK->save($con);
					}
				}
			}

			if ($this->collUnidadesGestorass !== null) {
				foreach ($this->collUnidadesGestorass as $referrerFK) {
					if (!$referrerFK->isDeleted()) {
						$affectedRows += $referrerFK->save($con);
					}
				}
			}

			if ($this->collVicePrefeitoss !== null) {
