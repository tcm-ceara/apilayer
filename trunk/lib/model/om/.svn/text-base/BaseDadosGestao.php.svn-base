			} else {

				$criteria->add(VicePrefeitosPeer::CD_MUNICIPIO, $this->cd_municipio);

				$criteria->add(VicePrefeitosPeer::DT_INI_GESTAO, $this->dt_ini_gestao);

				VicePrefeitosPeer::addSelectColumns($criteria);
				$this->collVicePrefeitoss = VicePrefeitosPeer::doSelect($criteria, $con);
			}
		} else {
			// criteria has no effect for a new object
			if (!$this->isNew()) {
				// the following code is to determine if a new query is
				// called for.  If the criteria is the same as the last
				// one, just return the collection.


				$criteria->add(VicePrefeitosPeer::CD_MUNICIPIO, $this->cd_municipio);


				$criteria->add(VicePrefeitosPeer::DT_INI_GESTAO, $this->dt_ini_gestao);

				VicePrefeitosPeer::addSelectColumns($criteria);
				if (!isset($this->lastVicePrefeitosCriteria) || !$this->lastVicePrefeitosCriteria->equals($criteria)) {
					$this->collVicePrefeitoss = VicePrefeitosPeer::doSelect($criteria, $con);
				}
			}
		}
		$this->lastVicePrefeitosCriteria = $criteria;
		return $this->collVicePrefeitoss;
	}

	/**
	 * Returns the number of related VicePrefeitos objects.
	 *
	 * @param      Criteria $criteria
	 * @param      boolean $distinct
	 * @param      PropelPDO $con
	 * @return     int Count of related VicePrefeitos objects.
	 * @throws     PropelException
	 */
	public function countVicePrefeitoss(Criteria $criteria = null, $distinct = false, PropelPDO $con = null)
	{
		if ($criteria === null) {
			$criteria = new Criteria(DadosGestaoPeer::DATABASE_NAME);
		} else {
			$criteria = clone $criteria;
		}

		if ($distinct) {
			$criteria->setDistinct();
		}

		$count = null;

		if ($this->collVicePrefeitoss === null) {
			if ($this->isNew()) {
				$count = 0;
			} else {

				$criteria->add(VicePrefeitosPeer::CD_MUNICIPIO, $this->cd_municipio);

				$criteria->add(VicePrefeitosPeer::DT_INI_GESTAO, $this->dt_ini_gestao);

				$count = VicePrefeitosPeer::doCount($criteria, false, $con);
			}
		} else {
			// criteria has no effect for a new object
			if (!$this->isNew()) {
				// the following code is to determine if a new query is
				// called for.  If the criteria is the same as the last
				// one, just return count of the collection.


				$criteria->add(VicePrefeitosPeer::CD_MUNICIPIO, $this->cd_municipio);


				$criteria->add(VicePrefeitosPeer::DT_INI_GESTAO, $this->dt_ini_gestao);

				if (!isset($this->lastVicePrefeitosCriteria) || !$this->lastVicePrefeitosCriteria->equals($criteria)) {
					$count = VicePrefeitosPeer::doCount($criteria, false, $con);
				} else {
					$count = count($this->collVicePrefeitoss);
				}
			} else {
				$count = count($this->collVicePrefeitoss);
			}
		}
		return $count;
	}

	/**
	 * Method called to associate a VicePrefeitos object to this object
	 * through the VicePrefeitos foreign key attribute.
	 *
	 * @param      VicePrefeitos $l VicePrefeitos
	 * @return     void
	 * @throws     PropelException
	 */
	public function addVicePrefeitos(VicePrefeitos $l)
	{
		if ($this->collVicePrefeitoss === null) {
			$this->initVicePrefeitoss();
		}
		if (!in_array($l, $this->collVicePrefeitoss, true)) { // only add it if the **same** object is not already associated
			array_push($this->collVicePrefeitoss, $l);
			$l->setDadosGestao($this);
		}
	}

	/**
	 * Resets all collections of referencing foreign keys.
	 *
	 * This method is a user-space workaround for PHP's inability to garbage collect objects
	 * with circular references.  This is currently necessary when using Propel in certain
	 * daemon or large-volumne/high-memory operations.
	 *
	 * @param      boolean $deep Whether to also clear the references on all associated objects.
	 */
	public function clearAllReferences($deep = false)
	{
		if ($deep) {
			if ($this->collGestSubstitutoss) {
				foreach ((array) $this->collGestSubstitutoss as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collUnidadesGestorass) {
				foreach ((array) $this->collUnidadesGestorass as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collVicePrefeitoss) {
				foreach ((array) $this->collVicePrefeitoss as $o) {
					$o->clearAllReferences($deep);
				}
			}
		} // if ($deep)

		$this->collGestSubstitutoss = null;
		$this->collUnidadesGestorass = null;
		$this->collVicePrefeitoss = null;
			$this->aGestores = null;
			$this->aTiposCargos = null;
	}

	// symfony_behaviors behavior
	
	/**
	 * Calls methods defined via {@link sfMixer}.
	 */
	public function __call($method, $arguments)
	{
	  if (!$callable = sfMixer::getCallable('BaseDadosGestao:'.$method))
	  {
	    throw new sfException(sprintf('Call to undefined method BaseDadosGestao::%s', $method));
	  }
	
	  array_unshift($arguments, $this);
	
	  return call_user_func_array($callable, $arguments);
	}

