	public static function retrieveByPK($cd_municipio, $dt_ini_obra_srv, $tp_obra_srv, $nu_obra_srv, $nu_rg_crea_eng, $dt_ini_resp_eng_ob, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_ini_obra_srv, (string) $tp_obra_srv, (string) $nu_obra_srv, (string) $nu_rg_crea_eng, (string) $dt_ini_resp_eng_ob));
 		if (null !== ($obj = RespEngenObrasPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(RespEngenObrasPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(RespEngenObrasPeer::DATABASE_NAME);
		$criteria->add(RespEngenObrasPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(RespEngenObrasPeer::DT_INI_OBRA_SRV, $dt_ini_obra_srv);
