	/**
	 * Retrieve object using using composite pkey values.
	 * @param      string $cd_municipio
	 * @param      int $dt_versao_orc
	 * @param      string $cd_orgao
	 * @param      string $cd_unid_orc
	 * @param      string $dt_emissao_ne
	 * @param      string $nu_nota_empenho
	 * @param      string $cd_tipo_rec_rc
	 * @param      string $nu_seq_rec_emp
	 * @param      PropelPDO $con
	 * @return     RecursosEmpenhos
	 */
	public static function retrieveByPK($cd_municipio, $dt_versao_orc, $cd_orgao, $cd_unid_orc, $dt_emissao_ne, $nu_nota_empenho, $cd_tipo_rec_rc, $nu_seq_rec_emp, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_versao_orc, (string) $cd_orgao, (string) $cd_unid_orc, (string) $dt_emissao_ne, (string) $nu_nota_empenho, (string) $cd_tipo_rec_rc, (string) $nu_seq_rec_emp));
 		if (null !== ($obj = RecursosEmpenhosPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(RecursosEmpenhosPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(RecursosEmpenhosPeer::DATABASE_NAME);
		$criteria->add(RecursosEmpenhosPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(RecursosEmpenhosPeer::DT_VERSAO_ORC, $dt_versao_orc);
		$criteria->add(RecursosEmpenhosPeer::CD_ORGAO, $cd_orgao);
		$criteria->add(RecursosEmpenhosPeer::CD_UNID_ORC, $cd_unid_orc);
		$criteria->add(RecursosEmpenhosPeer::DT_EMISSAO_NE, $dt_emissao_ne);
		$criteria->add(RecursosEmpenhosPeer::NU_NOTA_EMPENHO, $nu_nota_empenho);
		$criteria->add(RecursosEmpenhosPeer::CD_TIPO_REC_RC, $cd_tipo_rec_rc);
		$criteria->add(RecursosEmpenhosPeer::NU_SEQ_REC_EMP, $nu_seq_rec_emp);
		$v = RecursosEmpenhosPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
	}
