			} // if obj1 already loaded

				// Add objects for joined BensMunicipios rows

				$key2 = BensMunicipiosPeer::getPrimaryKeyHashFromRow($row, $startcol2);
				if ($key2 !== null) {
					$obj2 = BensMunicipiosPeer::getInstanceFromPool($key2);
					if (!$obj2) {
	
						$cls = BensMunicipiosPeer::getOMClass(false);

					$obj2 = new $cls();
					$obj2->hydrate($row, $startcol2);
					BensMunicipiosPeer::addInstanceToPool($obj2, $key2);
				} // if $obj2 already loaded

				// Add the $obj1 (EmpenhosBens) to the collection in $obj2 (BensMunicipios)
				$obj2->addEmpenhosBens($obj1);

			} // if joined row is not null

			$results[] = $obj1;
		}
		$stmt->closeCursor();
		return $results;
	}

