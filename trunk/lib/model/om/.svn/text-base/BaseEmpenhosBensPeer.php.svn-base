	/**
	 * Retrieve object using using composite pkey values.
	 * @param      string $cd_municipio
	 * @param      string $nu_registro_bem
	 * @param      int $dt_versao_orc
	 * @param      string $cd_orgao
	 * @param      string $cd_unid_orc
	 * @param      string $dt_emissao_ne
	 * @param      string $nu_nota_empenho
	 * @param      PropelPDO $con
	 * @return     EmpenhosBens
	 */
	public static function retrieveByPK($cd_municipio, $nu_registro_bem, $dt_versao_orc, $cd_orgao, $cd_unid_orc, $dt_emissao_ne, $nu_nota_empenho, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $nu_registro_bem, (string) $dt_versao_orc, (string) $cd_orgao, (string) $cd_unid_orc, (string) $dt_emissao_ne, (string) $nu_nota_empenho));
 		if (null !== ($obj = EmpenhosBensPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(EmpenhosBensPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(EmpenhosBensPeer::DATABASE_NAME);
		$criteria->add(EmpenhosBensPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(EmpenhosBensPeer::NU_REGISTRO_BEM, $nu_registro_bem);
		$criteria->add(EmpenhosBensPeer::DT_VERSAO_ORC, $dt_versao_orc);
		$criteria->add(EmpenhosBensPeer::CD_ORGAO, $cd_orgao);
		$criteria->add(EmpenhosBensPeer::CD_UNID_ORC, $cd_unid_orc);
		$criteria->add(EmpenhosBensPeer::DT_EMISSAO_NE, $dt_emissao_ne);
		$criteria->add(EmpenhosBensPeer::NU_NOTA_EMPENHO, $nu_nota_empenho);
		$v = EmpenhosBensPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
	}
