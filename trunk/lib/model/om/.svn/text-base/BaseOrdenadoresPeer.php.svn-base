	/**
	 * Method perform an INSERT on the database, given a Ordenadores or Criteria object.
	 *
	 * @param      mixed $values Criteria or Ordenadores object containing data that is used to create the INSERT statement.
	 * @param      PropelPDO $con the PropelPDO connection to use
	 * @return     mixed The new primary key.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	public static function doInsert($values, PropelPDO $con = null)
	{
    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseOrdenadoresPeer:doInsert:pre') as $sf_hook)
    {
      if (false !== $sf_hook_retval = call_user_func($sf_hook, 'BaseOrdenadoresPeer', $values, $con))
      {
        return $sf_hook_retval;
      }
    }

		if ($con === null) {
			$con = Propel::getConnection(OrdenadoresPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		if ($values instanceof Criteria) {
			$criteria = clone $values; // rename for clarity
		} else {
			$criteria = $values->buildCriteria(); // build Criteria from Ordenadores object
		}


		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		try {
			// use transaction because $criteria could contain info
			// for more than one table (I guess, conceivably)
			$con->beginTransaction();
			$pk = BasePeer::doInsert($criteria, $con);
			$con->commit();
		} catch(PropelException $e) {
			$con->rollBack();
			throw $e;
		}

    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseOrdenadoresPeer:doInsert:post') as $sf_hook)
    {
      call_user_func($sf_hook, 'BaseOrdenadoresPeer', $values, $con, $pk);
    }

		return $pk;
	}

	/**
	 * Method perform an UPDATE on the database, given a Ordenadores or Criteria object.
	 *
	 * @param      mixed $values Criteria or Ordenadores object containing data that is used to create the UPDATE statement.
	 * @param      PropelPDO $con The connection to use (specify PropelPDO connection object to exert more control over transactions).
	 * @return     int The number of affected rows (if supported by underlying database driver).
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	public static function doUpdate($values, PropelPDO $con = null)
	{
    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseOrdenadoresPeer:doUpdate:pre') as $sf_hook)
    {
      if (false !== $sf_hook_retval = call_user_func($sf_hook, 'BaseOrdenadoresPeer', $values, $con))
      {
        return $sf_hook_retval;
      }
    }

		if ($con === null) {
			$con = Propel::getConnection(OrdenadoresPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		$selectCriteria = new Criteria(self::DATABASE_NAME);

		if ($values instanceof Criteria) {
			$criteria = clone $values; // rename for clarity

			$comparison = $criteria->getComparison(OrdenadoresPeer::CD_MUNICIPIO);
			$selectCriteria->add(OrdenadoresPeer::CD_MUNICIPIO, $criteria->remove(OrdenadoresPeer::CD_MUNICIPIO), $comparison);

			$comparison = $criteria->getComparison(OrdenadoresPeer::DT_VERSAO_ORC);
			$selectCriteria->add(OrdenadoresPeer::DT_VERSAO_ORC, $criteria->remove(OrdenadoresPeer::DT_VERSAO_ORC), $comparison);

			$comparison = $criteria->getComparison(OrdenadoresPeer::CD_UNID_GEST_UG);
			$selectCriteria->add(OrdenadoresPeer::CD_UNID_GEST_UG, $criteria->remove(OrdenadoresPeer::CD_UNID_GEST_UG), $comparison);

			$comparison = $criteria->getComparison(OrdenadoresPeer::CD_ORGAO);
			$selectCriteria->add(OrdenadoresPeer::CD_ORGAO, $criteria->remove(OrdenadoresPeer::CD_ORGAO), $comparison);

			$comparison = $criteria->getComparison(OrdenadoresPeer::CD_UNID_ORC);
			$selectCriteria->add(OrdenadoresPeer::CD_UNID_ORC, $criteria->remove(OrdenadoresPeer::CD_UNID_ORC), $comparison);

			$comparison = $criteria->getComparison(OrdenadoresPeer::DT_INCLUSAO_UOG);
			$selectCriteria->add(OrdenadoresPeer::DT_INCLUSAO_UOG, $criteria->remove(OrdenadoresPeer::DT_INCLUSAO_UOG), $comparison);

			$comparison = $criteria->getComparison(OrdenadoresPeer::CD_CPF_SERV_MUNI);
			$selectCriteria->add(OrdenadoresPeer::CD_CPF_SERV_MUNI, $criteria->remove(OrdenadoresPeer::CD_CPF_SERV_MUNI), $comparison);

			$comparison = $criteria->getComparison(OrdenadoresPeer::CD_INGRESSO);
			$selectCriteria->add(OrdenadoresPeer::CD_INGRESSO, $criteria->remove(OrdenadoresPeer::CD_INGRESSO), $comparison);

			$comparison = $criteria->getComparison(OrdenadoresPeer::CD_VINCULO);
			$selectCriteria->add(OrdenadoresPeer::CD_VINCULO, $criteria->remove(OrdenadoresPeer::CD_VINCULO), $comparison);

			$comparison = $criteria->getComparison(OrdenadoresPeer::NU_EXPEDIENTE_NO);
			$selectCriteria->add(OrdenadoresPeer::NU_EXPEDIENTE_NO, $criteria->remove(OrdenadoresPeer::NU_EXPEDIENTE_NO), $comparison);

			$comparison = $criteria->getComparison(OrdenadoresPeer::DT_INI_GESTAO_ORD);
			$selectCriteria->add(OrdenadoresPeer::DT_INI_GESTAO_ORD, $criteria->remove(OrdenadoresPeer::DT_INI_GESTAO_ORD), $comparison);

		} else { // $values is Ordenadores object
			$criteria = $values->buildCriteria(); // gets full criteria
			$selectCriteria = $values->buildPkeyCriteria(); // gets criteria w/ primary key(s)
		}

		// set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$ret = BasePeer::doUpdate($selectCriteria, $criteria, $con);

    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseOrdenadoresPeer:doUpdate:post') as $sf_hook)
    {
      call_user_func($sf_hook, 'BaseOrdenadoresPeer', $values, $con, $ret);
    }

    return $ret;
	}

	/**
	 * Method to DELETE all rows from the ORDENADORES table.
	 *
	 * @return     int The number of affected rows (if supported by underlying database driver).
	 */
	public static function doDeleteAll($con = null)
	{
		if ($con === null) {
			$con = Propel::getConnection(OrdenadoresPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}
		$affectedRows = 0; // initialize var to track total num of affected rows
		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			$affectedRows += BasePeer::doDeleteAll(OrdenadoresPeer::TABLE_NAME, $con);
			// Because this db requires some delete cascade/set null emulation, we have to
			// clear the cached instance *after* the emulation has happened (since
			// instances get re-added by the select statement contained therein).
			OrdenadoresPeer::clearInstancePool();
			OrdenadoresPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

	/**
	 * Method perform a DELETE on the database, given a Ordenadores or Criteria object OR a primary key value.
	 *
	 * @param      mixed $values Criteria or Ordenadores object or primary key or array of primary keys
	 *              which is used to create the DELETE statement
	 * @param      PropelPDO $con the connection to use
	 * @return     int 	The number of affected rows (if supported by underlying database driver).  This includes CASCADE-related rows
	 *				if supported by native driver or if emulated using Propel.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	 public static function doDelete($values, PropelPDO $con = null)
	 {
		if ($con === null) {
			$con = Propel::getConnection(OrdenadoresPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		if ($values instanceof Criteria) {
			// invalidate the cache for all objects of this type, since we have no
			// way of knowing (without running a query) what objects should be invalidated
			// from the cache based on this Criteria.
			OrdenadoresPeer::clearInstancePool();
			// rename for clarity
			$criteria = clone $values;
		} elseif ($values instanceof Ordenadores) { // it's a model object
			// invalidate the cache for this single object
			OrdenadoresPeer::removeInstanceFromPool($values);
			// create criteria based on pk values
			$criteria = $values->buildPkeyCriteria();
		} else { // it's a primary key, or an array of pks
			$criteria = new Criteria(self::DATABASE_NAME);
			// primary key is composite; we therefore, expect
			// the primary key passed to be an array of pkey values
			if (count($values) == count($values, COUNT_RECURSIVE)) {
				// array is not multi-dimensional
				$values = array($values);
			}
			foreach ($values as $value) {
				$criterion = $criteria->getNewCriterion(OrdenadoresPeer::CD_MUNICIPIO, $value[0]);
				$criterion->addAnd($criteria->getNewCriterion(OrdenadoresPeer::DT_VERSAO_ORC, $value[1]));
				$criterion->addAnd($criteria->getNewCriterion(OrdenadoresPeer::CD_UNID_GEST_UG, $value[2]));
				$criterion->addAnd($criteria->getNewCriterion(OrdenadoresPeer::CD_ORGAO, $value[3]));
				$criterion->addAnd($criteria->getNewCriterion(OrdenadoresPeer::CD_UNID_ORC, $value[4]));
				$criterion->addAnd($criteria->getNewCriterion(OrdenadoresPeer::DT_INCLUSAO_UOG, $value[5]));
				$criterion->addAnd($criteria->getNewCriterion(OrdenadoresPeer::CD_CPF_SERV_MUNI, $value[6]));
				$criterion->addAnd($criteria->getNewCriterion(OrdenadoresPeer::CD_INGRESSO, $value[7]));
				$criterion->addAnd($criteria->getNewCriterion(OrdenadoresPeer::CD_VINCULO, $value[8]));
				$criterion->addAnd($criteria->getNewCriterion(OrdenadoresPeer::NU_EXPEDIENTE_NO, $value[9]));
				$criterion->addAnd($criteria->getNewCriterion(OrdenadoresPeer::DT_INI_GESTAO_ORD, $value[10]));
				$criteria->addOr($criterion);
				// we can invalidate the cache for this single PK
				OrdenadoresPeer::removeInstanceFromPool($value);
			}
		}

		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$affectedRows = 0; // initialize var to track total num of affected rows

		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			
			$affectedRows += BasePeer::doDelete($criteria, $con);
			OrdenadoresPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

	/**
	 * Validates all modified columns of given Ordenadores object.
	 * If parameter $columns is either a single column name or an array of column names
	 * than only those columns are validated.
	 *
	 * NOTICE: This does not apply to primary or foreign keys for now.
	 *
	 * @param      Ordenadores $obj The object to validate.
	 * @param      mixed $cols Column name or array of column names.
	 *
	 * @return     mixed TRUE if all columns are valid or the error message of the first invalid column.
	 */
	public static function doValidate(Ordenadores $obj, $cols = null)
	{
		$columns = array();

		if ($cols) {
			$dbMap = Propel::getDatabaseMap(OrdenadoresPeer::DATABASE_NAME);
			$tableMap = $dbMap->getTable(OrdenadoresPeer::TABLE_NAME);

			if (! is_array($cols)) {
				$cols = array($cols);
			}

			foreach ($cols as $colName) {
				if ($tableMap->containsColumn($colName)) {
					$get = 'get' . $tableMap->getColumn($colName)->getPhpName();
					$columns[$colName] = $obj->$get();
				}
			}
		} else {

		}

		return BasePeer::doValidate(OrdenadoresPeer::DATABASE_NAME, OrdenadoresPeer::TABLE_NAME, $columns);
	}

	/**
	 * Retrieve object using using composite pkey values.
	 * @param      string $cd_municipio
	 * @param      int $dt_versao_orc
	 * @param      int $cd_unid_gest_ug
	 * @param      string $cd_orgao
	 * @param      string $cd_unid_orc
	 * @param      string $dt_inclusao_uog
	 * @param      string $cd_cpf_serv_muni
	 * @param      string $cd_ingresso
	 * @param      string $cd_vinculo
	 * @param      string $nu_expediente_no
	 * @param      string $dt_ini_gestao_ord
	 * @param      PropelPDO $con
	 * @return     Ordenadores
	 */
	public static function retrieveByPK($cd_municipio, $dt_versao_orc, $cd_unid_gest_ug, $cd_orgao, $cd_unid_orc, $dt_inclusao_uog, $cd_cpf_serv_muni, $cd_ingresso, $cd_vinculo, $nu_expediente_no, $dt_ini_gestao_ord, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_versao_orc, (string) $cd_unid_gest_ug, (string) $cd_orgao, (string) $cd_unid_orc, (string) $dt_inclusao_uog, (string) $cd_cpf_serv_muni, (string) $cd_ingresso, (string) $cd_vinculo, (string) $nu_expediente_no, (string) $dt_ini_gestao_ord));
 		if (null !== ($obj = OrdenadoresPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(OrdenadoresPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(OrdenadoresPeer::DATABASE_NAME);
		$criteria->add(OrdenadoresPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(OrdenadoresPeer::DT_VERSAO_ORC, $dt_versao_orc);
		$criteria->add(OrdenadoresPeer::CD_UNID_GEST_UG, $cd_unid_gest_ug);
		$criteria->add(OrdenadoresPeer::CD_ORGAO, $cd_orgao);
		$criteria->add(OrdenadoresPeer::CD_UNID_ORC, $cd_unid_orc);
		$criteria->add(OrdenadoresPeer::DT_INCLUSAO_UOG, $dt_inclusao_uog);
		$criteria->add(OrdenadoresPeer::CD_CPF_SERV_MUNI, $cd_cpf_serv_muni);
		$criteria->add(OrdenadoresPeer::CD_INGRESSO, $cd_ingresso);
		$criteria->add(OrdenadoresPeer::CD_VINCULO, $cd_vinculo);
		$criteria->add(OrdenadoresPeer::NU_EXPEDIENTE_NO, $nu_expediente_no);
		$criteria->add(OrdenadoresPeer::DT_INI_GESTAO_ORD, $dt_ini_gestao_ord);
		$v = OrdenadoresPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
	}
