	public static function doSelectJoinAllExceptUnidOrcUg(Criteria $criteria, $con = null, $join_behavior = Criteria::LEFT_JOIN)
	{
		$criteria = clone $criteria;

		// Set the correct dbName if it has not been overridden
		// $criteria->getDbName() will return the same object if not set to another value
		// so == check is okay and faster
		if ($criteria->getDbName() == Propel::getDefaultDB()) {
			$criteria->setDbName(self::DATABASE_NAME);
		}

		OrdenadoresPeer::addSelectColumns($criteria);
		$startcol2 = (OrdenadoresPeer::NUM_COLUMNS - OrdenadoresPeer::NUM_LAZY_LOAD_COLUMNS);

		AgentesPublicosPeer::addSelectColumns($criteria);
		$startcol3 = $startcol2 + (AgentesPublicosPeer::NUM_COLUMNS - AgentesPublicosPeer::NUM_LAZY_LOAD_COLUMNS);

		$criteria->addMultipleJoin(array(
        array(OrdenadoresPeer::CD_CPF_SERV_MUNI, AgentesPublicosPeer::CD_CPF_SERV_MUNI),
        array(OrdenadoresPeer::CD_INGRESSO, AgentesPublicosPeer::CD_INGRESSO),
        array(OrdenadoresPeer::CD_MUNICIPIO, AgentesPublicosPeer::CD_MUNICIPIO),
        array(OrdenadoresPeer::CD_ORGAO, AgentesPublicosPeer::CD_ORGAO),
        array(OrdenadoresPeer::CD_UNID_ORC, AgentesPublicosPeer::CD_UNID_ORC),
        array(OrdenadoresPeer::CD_VINCULO, AgentesPublicosPeer::CD_VINCULO),
        array(OrdenadoresPeer::DT_VERSAO_ORC, AgentesPublicosPeer::DT_VERSAO_ORC),
        array(OrdenadoresPeer::NU_EXPEDIENTE_NO, AgentesPublicosPeer::NU_EXPEDIENTE_NO),
      ), $join_behavior);

		// symfony_behaviors behavior
		foreach (sfMixer::getCallables(self::getMixerPreSelectHook(__FUNCTION__)) as $sf_hook)
		{
		  call_user_func($sf_hook, 'BaseOrdenadoresPeer', $criteria, $con);
		}


		$stmt = BasePeer::doSelect($criteria, $con);
		$results = array();

		while ($row = $stmt->fetch(PDO::FETCH_NUM)) {
			$key1 = OrdenadoresPeer::getPrimaryKeyHashFromRow($row, 0);
			if (null !== ($obj1 = OrdenadoresPeer::getInstanceFromPool($key1))) {
				// We no longer rehydrate the object, since this can cause data loss.
				// See http://propel.phpdb.org/trac/ticket/509
				// $obj1->hydrate($row, 0, true); // rehydrate
			} else {
				$cls = OrdenadoresPeer::getOMClass(false);

				$obj1 = new $cls();
				$obj1->hydrate($row);
				OrdenadoresPeer::addInstanceToPool($obj1, $key1);
			} // if obj1 already loaded

				// Add objects for joined AgentesPublicos rows

				$key2 = AgentesPublicosPeer::getPrimaryKeyHashFromRow($row, $startcol2);
				if ($key2 !== null) {
					$obj2 = AgentesPublicosPeer::getInstanceFromPool($key2);
					if (!$obj2) {
	
