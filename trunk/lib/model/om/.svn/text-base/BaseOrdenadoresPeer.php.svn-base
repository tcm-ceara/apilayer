					$obj2 = new $cls();
					$obj2->hydrate($row, $startcol2);
					AgentesPublicosPeer::addInstanceToPool($obj2, $key2);
				} // if $obj2 already loaded

				// Add the $obj1 (Ordenadores) to the collection in $obj2 (AgentesPublicos)
				$obj2->addOrdenadores($obj1);

			} // if joined row is not null

			$results[] = $obj1;
		}
		$stmt->closeCursor();
		return $results;
	}


