		}


		$stmt = BasePeer::doSelect($criteria, $con);
		$results = array();

		while ($row = $stmt->fetch(PDO::FETCH_NUM)) {
			$key1 = MembrosComLicitPeer::getPrimaryKeyHashFromRow($row, 0);
			if (null !== ($obj1 = MembrosComLicitPeer::getInstanceFromPool($key1))) {
