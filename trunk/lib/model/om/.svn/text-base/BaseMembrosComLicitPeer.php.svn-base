				$obj1 = new $cls();
				$obj1->hydrate($row);
				MembrosComLicitPeer::addInstanceToPool($obj1, $key1);
			} // if obj1 already loaded

				// Add objects for joined DadosMembrosComLicit rows

				$key2 = DadosMembrosComLicitPeer::getPrimaryKeyHashFromRow($row, $startcol2);
				if ($key2 !== null) {
					$obj2 = DadosMembrosComLicitPeer::getInstanceFromPool($key2);
					if (!$obj2) {
	
						$cls = DadosMembrosComLicitPeer::getOMClass(false);

					$obj2 = new $cls();
					$obj2->hydrate($row, $startcol2);
					DadosMembrosComLicitPeer::addInstanceToPool($obj2, $key2);
				} // if $obj2 already loaded

				// Add the $obj1 (MembrosComLicit) to the collection in $obj2 (DadosMembrosComLicit)
				$obj2->addMembrosComLicit($obj1);

			} // if joined row is not null

			$results[] = $obj1;
		}
		$stmt->closeCursor();
