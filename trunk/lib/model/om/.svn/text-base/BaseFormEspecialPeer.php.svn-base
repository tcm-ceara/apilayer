			$comparison = $criteria->getComparison(FormEspecialPeer::CD_ORGAO);
			$selectCriteria->add(FormEspecialPeer::CD_ORGAO, $criteria->remove(FormEspecialPeer::CD_ORGAO), $comparison);

			$comparison = $criteria->getComparison(FormEspecialPeer::CD_UNID_ORC);
			$selectCriteria->add(FormEspecialPeer::CD_UNID_ORC, $criteria->remove(FormEspecialPeer::CD_UNID_ORC), $comparison);

			$comparison = $criteria->getComparison(FormEspecialPeer::DT_REF);
			$selectCriteria->add(FormEspecialPeer::DT_REF, $criteria->remove(FormEspecialPeer::DT_REF), $comparison);

		} else { // $values is FormEspecial object
			$criteria = $values->buildCriteria(); // gets full criteria
			$selectCriteria = $values->buildPkeyCriteria(); // gets criteria w/ primary key(s)
		}

		// set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$ret = BasePeer::doUpdate($selectCriteria, $criteria, $con);

    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseFormEspecialPeer:doUpdate:post') as $sf_hook)
    {
      call_user_func($sf_hook, 'BaseFormEspecialPeer', $values, $con, $ret);
    }

    return $ret;
	}

	/**
	 * Method to DELETE all rows from the FORM_ESPECIAL table.
	 *
	 * @return     int The number of affected rows (if supported by underlying database driver).
	 */
	public static function doDeleteAll($con = null)
	{
		if ($con === null) {
			$con = Propel::getConnection(FormEspecialPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}
		$affectedRows = 0; // initialize var to track total num of affected rows
		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			$affectedRows += BasePeer::doDeleteAll(FormEspecialPeer::TABLE_NAME, $con);
			// Because this db requires some delete cascade/set null emulation, we have to
			// clear the cached instance *after* the emulation has happened (since
			// instances get re-added by the select statement contained therein).
			FormEspecialPeer::clearInstancePool();
			FormEspecialPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

	/**
	 * Method perform a DELETE on the database, given a FormEspecial or Criteria object OR a primary key value.
	 *
	 * @param      mixed $values Criteria or FormEspecial object or primary key or array of primary keys
	 *              which is used to create the DELETE statement
	 * @param      PropelPDO $con the connection to use
	 * @return     int 	The number of affected rows (if supported by underlying database driver).  This includes CASCADE-related rows
	 *				if supported by native driver or if emulated using Propel.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	 public static function doDelete($values, PropelPDO $con = null)
	 {
		if ($con === null) {
			$con = Propel::getConnection(FormEspecialPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		if ($values instanceof Criteria) {
			// invalidate the cache for all objects of this type, since we have no
			// way of knowing (without running a query) what objects should be invalidated
			// from the cache based on this Criteria.
			FormEspecialPeer::clearInstancePool();
			// rename for clarity
			$criteria = clone $values;
		} elseif ($values instanceof FormEspecial) { // it's a model object
			// invalidate the cache for this single object
			FormEspecialPeer::removeInstanceFromPool($values);
			// create criteria based on pk values
			$criteria = $values->buildPkeyCriteria();
		} else { // it's a primary key, or an array of pks
			$criteria = new Criteria(self::DATABASE_NAME);
			// primary key is composite; we therefore, expect
			// the primary key passed to be an array of pkey values
			if (count($values) == count($values, COUNT_RECURSIVE)) {
				// array is not multi-dimensional
				$values = array($values);
			}
			foreach ($values as $value) {
				$criterion = $criteria->getNewCriterion(FormEspecialPeer::CD_MUNICIPIO, $value[0]);
				$criterion->addAnd($criteria->getNewCriterion(FormEspecialPeer::DT_EXERCICIO, $value[1]));
				$criterion->addAnd($criteria->getNewCriterion(FormEspecialPeer::CD_ORGAO, $value[2]));
				$criterion->addAnd($criteria->getNewCriterion(FormEspecialPeer::CD_UNID_ORC, $value[3]));
				$criterion->addAnd($criteria->getNewCriterion(FormEspecialPeer::DT_REF, $value[4]));
				$criteria->addOr($criterion);
				// we can invalidate the cache for this single PK
				FormEspecialPeer::removeInstanceFromPool($value);
			}
		}

		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$affectedRows = 0; // initialize var to track total num of affected rows

		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			
			$affectedRows += BasePeer::doDelete($criteria, $con);
			FormEspecialPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

	/**
	 * Validates all modified columns of given FormEspecial object.
	 * If parameter $columns is either a single column name or an array of column names
	 * than only those columns are validated.
	 *
	 * NOTICE: This does not apply to primary or foreign keys for now.
	 *
	 * @param      FormEspecial $obj The object to validate.
	 * @param      mixed $cols Column name or array of column names.
	 *
	 * @return     mixed TRUE if all columns are valid or the error message of the first invalid column.
	 */
	public static function doValidate(FormEspecial $obj, $cols = null)
	{
		$columns = array();

		if ($cols) {
			$dbMap = Propel::getDatabaseMap(FormEspecialPeer::DATABASE_NAME);
			$tableMap = $dbMap->getTable(FormEspecialPeer::TABLE_NAME);

			if (! is_array($cols)) {
				$cols = array($cols);
			}

			foreach ($cols as $colName) {
				if ($tableMap->containsColumn($colName)) {
					$get = 'get' . $tableMap->getColumn($colName)->getPhpName();
					$columns[$colName] = $obj->$get();
				}
			}
		} else {

		}

		return BasePeer::doValidate(FormEspecialPeer::DATABASE_NAME, FormEspecialPeer::TABLE_NAME, $columns);
	}

	/**
	 * Retrieve object using using composite pkey values.
	 * @param      string $cd_municipio
	 * @param      int $dt_exercicio
	 * @param      string $cd_orgao
	 * @param      string $cd_unid_orc
	 * @param      int $dt_ref
	 * @param      PropelPDO $con
	 * @return     FormEspecial
	 */
	public static function retrieveByPK($cd_municipio, $dt_exercicio, $cd_orgao, $cd_unid_orc, $dt_ref, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_exercicio, (string) $cd_orgao, (string) $cd_unid_orc, (string) $dt_ref));
 		if (null !== ($obj = FormEspecialPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(FormEspecialPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(FormEspecialPeer::DATABASE_NAME);
		$criteria->add(FormEspecialPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(FormEspecialPeer::DT_EXERCICIO, $dt_exercicio);
		$criteria->add(FormEspecialPeer::CD_ORGAO, $cd_orgao);
		$criteria->add(FormEspecialPeer::CD_UNID_ORC, $cd_unid_orc);
		$criteria->add(FormEspecialPeer::DT_REF, $dt_ref);
		$v = FormEspecialPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
	}
