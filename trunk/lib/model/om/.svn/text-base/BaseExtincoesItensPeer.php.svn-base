			foreach ($values as $value) {
				$criterion = $criteria->getNewCriterion(ExtincoesItensPeer::CD_MUNICIPIO, $value[0]);
				$criterion->addAnd($criteria->getNewCriterion(ExtincoesItensPeer::CD_ITEM_IR, $value[1]));
