	 */
	public static function retrieveByPK($cd_municipio, $cd_item_ir, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $cd_item_ir));
 		if (null !== ($obj = ExtincoesItensPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(ExtincoesItensPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(ExtincoesItensPeer::DATABASE_NAME);
		$criteria->add(ExtincoesItensPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(ExtincoesItensPeer::CD_ITEM_IR, $cd_item_ir);
		$v = ExtincoesItensPeer::doSelect($criteria, $con);

		return !empty($v) ? $v[0] : null;
