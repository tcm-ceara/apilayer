	 * through the Pensionistas foreign key attribute.
	 *
	 * @param      Pensionistas $l Pensionistas
	 * @return     void
	 * @throws     PropelException
	 */
	public function addPensionistas(Pensionistas $l)
	{
		if ($this->collPensionistass === null) {
			$this->initPensionistass();
		}
		if (!in_array($l, $this->collPensionistass, true)) { // only add it if the **same** object is not already associated
			array_push($this->collPensionistass, $l);
			$l->setServidoresMunicipais($this);
		}
	}

	/**
	 * Resets all collections of referencing foreign keys.
	 *
	 * This method is a user-space workaround for PHP's inability to garbage collect objects
	 * with circular references.  This is currently necessary when using Propel in certain
	 * daemon or large-volumne/high-memory operations.
	 *
	 * @param      boolean $deep Whether to also clear the references on all associated objects.
	 */
	public function clearAllReferences($deep = false)
	{
		if ($deep) {
			if ($this->collDadosPessoaisServ2s) {
				foreach ((array) $this->collDadosPessoaisServ2s as $o) {
					$o->clearAllReferences($deep);
				}
			}
			if ($this->collPensionistass) {
				foreach ((array) $this->collPensionistass as $o) {
					$o->clearAllReferences($deep);
				}
			}
		} // if ($deep)

		$this->collDadosPessoaisServ2s = null;
		$this->collPensionistass = null;
			$this->aDadosPessoaisServ = null;
			$this->aSitFuncionais = null;
			$this->aVinculos = null;
			$this->aGrausInstrucao = null;
			$this->aFormaIngresso = null;
			$this->aUnidadesOrcamentarias = null;
	}

	// symfony_behaviors behavior
	
	/**
	 * Calls methods defined via {@link sfMixer}.
	 */
	public function __call($method, $arguments)
	{
	  if (!$callable = sfMixer::getCallable('BaseServidoresMunicipais:'.$method))
	  {
	    throw new sfException(sprintf('Call to undefined method BaseServidoresMunicipais::%s', $method));
	  }
	
	  array_unshift($arguments, $this);
	
	  return call_user_func_array($callable, $arguments);
	}

} // BaseServidoresMunicipais
