	public function getVereadores(PropelPDO $con = null)
	{
		if ($this->aVereadores === null && (($this->dt_ini_mandato_ve !== "" && $this->dt_ini_mandato_ve !== null) && ($this->nu_cpf_vereador !== "" && $this->nu_cpf_vereador !== null))) {
			$c = new Criteria(VereadoresPeer::DATABASE_NAME);
			$c->add(VereadoresPeer::DT_INI_MANDATO_VE, $this->dt_ini_mandato_ve);
			$c->add(VereadoresPeer::NU_CPF_VEREADOR, $this->nu_cpf_vereador);
			$this->aVereadores = VereadoresPeer::doSelectOne($c, $con);
			/* The following can be used additionally to
			   guarantee the related object contains a reference
			   to this object.  This level of coupling may, however, be
			   undesirable since it could result in an only partially populated collection
			   in the referenced object.
			   $this->aVereadores->addSubsidiosVereadoress($this);
			 */
		}
		return $this->aVereadores;
	}

