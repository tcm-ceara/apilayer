		} else {
			$this->setDtIniMandatoVe($v->getDtIniMandatoVe());
		}

		if ($v === null) {
			$this->setNuCpfVereador(NULL);
		} else {
			$this->setNuCpfVereador($v->getNuCpfVereador());
		}

		$this->aVereadores = $v;

		// Add binding for other direction of this n:n relationship.
		// If this object has already been added to the Vereadores object, it will not be re-added.
		if ($v !== null) {
			$v->addSubsidiosVereadores($this);
		}

		return $this;
	}


