	/**
	 * Method perform a DELETE on the database, given a LiquidacoesFolhas or Criteria object OR a primary key value.
	 *
	 * @param      mixed $values Criteria or LiquidacoesFolhas object or primary key or array of primary keys
	 *              which is used to create the DELETE statement
	 * @param      PropelPDO $con the connection to use
	 * @return     int 	The number of affected rows (if supported by underlying database driver).  This includes CASCADE-related rows
	 *				if supported by native driver or if emulated using Propel.
	 * @throws     PropelException Any exceptions caught during processing will be
	 *		 rethrown wrapped into a PropelException.
	 */
	 public static function doDelete($values, PropelPDO $con = null)
	 {
		if ($con === null) {
			$con = Propel::getConnection(LiquidacoesFolhasPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		if ($values instanceof Criteria) {
			// invalidate the cache for all objects of this type, since we have no
			// way of knowing (without running a query) what objects should be invalidated
			// from the cache based on this Criteria.
			LiquidacoesFolhasPeer::clearInstancePool();
			// rename for clarity
			$criteria = clone $values;
		} elseif ($values instanceof LiquidacoesFolhas) { // it's a model object
			// invalidate the cache for this single object
			LiquidacoesFolhasPeer::removeInstanceFromPool($values);
			// create criteria based on pk values
			$criteria = $values->buildPkeyCriteria();
		} else { // it's a primary key, or an array of pks
			$criteria = new Criteria(self::DATABASE_NAME);
			// primary key is composite; we therefore, expect
			// the primary key passed to be an array of pkey values
			if (count($values) == count($values, COUNT_RECURSIVE)) {
				// array is not multi-dimensional
				$values = array($values);
			}
			foreach ($values as $value) {
				$criterion = $criteria->getNewCriterion(LiquidacoesFolhasPeer::CD_MUNICIPIO, $value[0]);
				$criterion->addAnd($criteria->getNewCriterion(LiquidacoesFolhasPeer::DT_VERSAO_ORC, $value[1]));
				$criterion->addAnd($criteria->getNewCriterion(LiquidacoesFolhasPeer::CD_ORGAO, $value[2]));
				$criterion->addAnd($criteria->getNewCriterion(LiquidacoesFolhasPeer::CD_UNID_ORC, $value[3]));
				$criterion->addAnd($criteria->getNewCriterion(LiquidacoesFolhasPeer::DT_EMISSAO_NE, $value[4]));
				$criterion->addAnd($criteria->getNewCriterion(LiquidacoesFolhasPeer::NU_NOTA_EMPENHO, $value[5]));
				$criterion->addAnd($criteria->getNewCriterion(LiquidacoesFolhasPeer::DT_LIQUID_LIQ, $value[6]));
				$criterion->addAnd($criteria->getNewCriterion(LiquidacoesFolhasPeer::DT_COMPET_FL, $value[7]));
				$criterion->addAnd($criteria->getNewCriterion(LiquidacoesFolhasPeer::TP_FOLHA, $value[8]));
				$criterion->addAnd($criteria->getNewCriterion(LiquidacoesFolhasPeer::DT_EMISSAO_FL, $value[9]));
				$criteria->addOr($criterion);
				// we can invalidate the cache for this single PK
				LiquidacoesFolhasPeer::removeInstanceFromPool($value);
			}
		}

		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$affectedRows = 0; // initialize var to track total num of affected rows

		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			
			$affectedRows += BasePeer::doDelete($criteria, $con);
			LiquidacoesFolhasPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
		}
	}

