			$selectCriteria->add(LiquidacoesFolhasPeer::NU_NOTA_EMPENHO, $criteria->remove(LiquidacoesFolhasPeer::NU_NOTA_EMPENHO), $comparison);

			$comparison = $criteria->getComparison(LiquidacoesFolhasPeer::DT_LIQUID_LIQ);
			$selectCriteria->add(LiquidacoesFolhasPeer::DT_LIQUID_LIQ, $criteria->remove(LiquidacoesFolhasPeer::DT_LIQUID_LIQ), $comparison);

			$comparison = $criteria->getComparison(LiquidacoesFolhasPeer::DT_COMPET_FL);
			$selectCriteria->add(LiquidacoesFolhasPeer::DT_COMPET_FL, $criteria->remove(LiquidacoesFolhasPeer::DT_COMPET_FL), $comparison);

			$comparison = $criteria->getComparison(LiquidacoesFolhasPeer::TP_FOLHA);
			$selectCriteria->add(LiquidacoesFolhasPeer::TP_FOLHA, $criteria->remove(LiquidacoesFolhasPeer::TP_FOLHA), $comparison);

			$comparison = $criteria->getComparison(LiquidacoesFolhasPeer::DT_EMISSAO_FL);
			$selectCriteria->add(LiquidacoesFolhasPeer::DT_EMISSAO_FL, $criteria->remove(LiquidacoesFolhasPeer::DT_EMISSAO_FL), $comparison);

		} else { // $values is LiquidacoesFolhas object
			$criteria = $values->buildCriteria(); // gets full criteria
			$selectCriteria = $values->buildPkeyCriteria(); // gets criteria w/ primary key(s)
		}

		// set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$ret = BasePeer::doUpdate($selectCriteria, $criteria, $con);

