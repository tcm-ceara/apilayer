	public static function doInsert($values, PropelPDO $con = null)
	{
    // symfony_behaviors behavior
    foreach (sfMixer::getCallables('BaseLiquidacoesFolhasPeer:doInsert:pre') as $sf_hook)
    {
      if (false !== $sf_hook_retval = call_user_func($sf_hook, 'BaseLiquidacoesFolhasPeer', $values, $con))
      {
        return $sf_hook_retval;
      }
    }

		if ($con === null) {
