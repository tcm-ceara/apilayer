	public static function retrieveByPK($cd_municipio, $dt_versao_orc, $cd_orgao, $cd_unid_orc, $cd_conta_ctx, $nu_banco_cb, $nu_agencia_cb, $nu_conta_corrente_cb, $nu_docdesp_xd, PropelPDO $con = null) {
		$key = serialize(array((string) $cd_municipio, (string) $dt_versao_orc, (string) $cd_orgao, (string) $cd_unid_orc, (string) $cd_conta_ctx, (string) $nu_banco_cb, (string) $nu_agencia_cb, (string) $nu_conta_corrente_cb, (string) $nu_docdesp_xd));
 		if (null !== ($obj = DespesasExtraOrcPeer::getInstanceFromPool($key))) {
 			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(DespesasExtraOrcPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}
		$criteria = new Criteria(DespesasExtraOrcPeer::DATABASE_NAME);
		$criteria->add(DespesasExtraOrcPeer::CD_MUNICIPIO, $cd_municipio);
		$criteria->add(DespesasExtraOrcPeer::DT_VERSAO_ORC, $dt_versao_orc);
		$criteria->add(DespesasExtraOrcPeer::CD_ORGAO, $cd_orgao);
		$criteria->add(DespesasExtraOrcPeer::CD_UNID_ORC, $cd_unid_orc);
		$criteria->add(DespesasExtraOrcPeer::CD_CONTA_CTX, $cd_conta_ctx);
