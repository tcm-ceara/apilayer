	 public static function doDelete($values, PropelPDO $con = null)
	 {
		if ($con === null) {
			$con = Propel::getConnection(DespesasExtraOrcPeer::DATABASE_NAME, Propel::CONNECTION_WRITE);
		}

		if ($values instanceof Criteria) {
			// invalidate the cache for all objects of this type, since we have no
			// way of knowing (without running a query) what objects should be invalidated
			// from the cache based on this Criteria.
			DespesasExtraOrcPeer::clearInstancePool();
			// rename for clarity
			$criteria = clone $values;
		} elseif ($values instanceof DespesasExtraOrc) { // it's a model object
			// invalidate the cache for this single object
			DespesasExtraOrcPeer::removeInstanceFromPool($values);
			// create criteria based on pk values
			$criteria = $values->buildPkeyCriteria();
		} else { // it's a primary key, or an array of pks
			$criteria = new Criteria(self::DATABASE_NAME);
			// primary key is composite; we therefore, expect
			// the primary key passed to be an array of pkey values
			if (count($values) == count($values, COUNT_RECURSIVE)) {
				// array is not multi-dimensional
				$values = array($values);
			}
			foreach ($values as $value) {
				$criterion = $criteria->getNewCriterion(DespesasExtraOrcPeer::CD_MUNICIPIO, $value[0]);
				$criterion->addAnd($criteria->getNewCriterion(DespesasExtraOrcPeer::DT_VERSAO_ORC, $value[1]));
				$criterion->addAnd($criteria->getNewCriterion(DespesasExtraOrcPeer::CD_ORGAO, $value[2]));
				$criterion->addAnd($criteria->getNewCriterion(DespesasExtraOrcPeer::CD_UNID_ORC, $value[3]));
				$criterion->addAnd($criteria->getNewCriterion(DespesasExtraOrcPeer::CD_CONTA_CTX, $value[4]));
				$criterion->addAnd($criteria->getNewCriterion(DespesasExtraOrcPeer::NU_BANCO_CB, $value[5]));
				$criterion->addAnd($criteria->getNewCriterion(DespesasExtraOrcPeer::NU_AGENCIA_CB, $value[6]));
				$criterion->addAnd($criteria->getNewCriterion(DespesasExtraOrcPeer::NU_CONTA_CORRENTE_CB, $value[7]));
				$criterion->addAnd($criteria->getNewCriterion(DespesasExtraOrcPeer::NU_DOCDESP_XD, $value[8]));
				$criteria->addOr($criterion);
				// we can invalidate the cache for this single PK
				DespesasExtraOrcPeer::removeInstanceFromPool($value);
			}
		}

		// Set the correct dbName
		$criteria->setDbName(self::DATABASE_NAME);

		$affectedRows = 0; // initialize var to track total num of affected rows

		try {
			// use transaction because $criteria could contain info
			// for more than one table or we could emulating ON DELETE CASCADE, etc.
			$con->beginTransaction();
			
			$affectedRows += BasePeer::doDelete($criteria, $con);
			DespesasExtraOrcPeer::clearRelatedInstancePool();
			$con->commit();
			return $affectedRows;
		} catch (PropelException $e) {
			$con->rollBack();
			throw $e;
