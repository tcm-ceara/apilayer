				$this->collCatEcoProjAtivs = CatEcoProjAtivPeer::doSelectJoinOrcDespCatEco($criteria, $con, $join_behavior);
			}
		}
		$this->lastCatEcoProjAtivCriteria = $criteria;

		return $this->collCatEcoProjAtivs;
	}

	/**
