			}
		} // if ($deep)

		$this->collCatEcoProjAtivs = null;
			$this->aSubfuncoes = null;
			$this->aTiposOrcamentos = null;
			$this->aUnidadesOrcamentarias = null;
			$this->aProgramas = null;
	}

